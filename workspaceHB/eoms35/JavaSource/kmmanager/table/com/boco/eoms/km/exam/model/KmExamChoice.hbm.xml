<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.exam.model.KmExamChoice"
            table="KM_EXAM_CHOICE"
    >

    <id
        name="choiceID"
            column="CHOICE_ID"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="questionsID"
            	column="QUESTIONS_ID"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="content"
            	column="CONTENT"
                length="100"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="orderBy"
            	column="ORDER_BY"
                length="5"
                not-null="true"
                unique="false"
        >

        </property>


    </class>    

</hibernate-mapping>

