<?xml version="1.0" encoding="UTF-8"?>
<WorkFlowRule name="SafetyProblem" description="安全问题流程">
   <flow-define id="SafetyProblemMainFlowProcess" description="安全问题流程">
     <phase-id id="receive" type="receive">
       <to-phase-id id="TransmitMoreTask" role="" condition="" name="派发"/>
       <to-phase-id id="DraftHumTask" role="" condition="" name="保存草稿"/>   
     </phase-id>
     <phase-id id="DraftHumTask" name="草稿" type="operate">   
        <to-phase-id id="TransmitMoreTask" role="" condition="" name="派发"/>
        <to-phase-id id="TaskCreateAuditHumTask" role="" condition="" name="审核"/>
     </phase-id> 
     <phase-id id="TaskCreateAuditHumTask" name="任务创建审批中" type="operate">  
        <to-phase-id id="ExcuteHumTask" role="" condition="" name="派发审批通过"/>
        <to-phase-id id="ByRejectHumTask" role="" condition="" name="派发审批不通过"/>
     </phase-id>     
     <phase-id id="ByRejectHumTask" name="驳回" type="operate">
        <to-phase-id id="TaskCreateAuditHumTask" role="" condition="" name="重新派发"/>
     </phase-id>
     <phase-id id="ExcuteHumTask" name="任务执行中" type="operate">
     	<to-phase-id id="TaskCompleteAuditHumTask" role="" condition="" name="执行完成/送审"/>
     	<to-phase-id id="AffirmHumTask" role="" condition="" name="执行完成/无需审批"/>
     </phase-id>
     <phase-id id="AffirmHumTask" name="处理回复中" type="operate">
     	<to-phase-id id="ExcuteHumTask" role="" condition="" name="处理回复不通过"/>
        <to-phase-id id="Receive" role="" condition="" name="处理回复通过"/>
     </phase-id>
     <phase-id id="Receive" name="回复" type="operate">
     	<to-phase-id id="HoldHumTask" role="" condition="" name="归档"/>
     </phase-id>
     <phase-id id="HoldHumTask" name="归档" type="operate">
     </phase-id>
     
  </flow-define>
</WorkFlowRule>