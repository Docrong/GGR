<?xml version="1.0" encoding="UTF-8"?>
<WorkFlowRule name="BusinessImplement" description="业务开通流程">
    <flow-define id="BusinessImplementProcess" description="业务开通流程" tasknamespace="http://BusinessImplementProcess"
                 linkservicename="iBusinessImplementLinkManager" dictfilename="dict-sheet-businessimplement"
                 name="BusinessImplement">
        <phase-id id="receive" name="新建" type="receive" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="ImplementDealTask" role="dealPerformer@" condition="${operateType}==" operatetype=""
                         workflowdisplay="false" isrejecttask="false" roleid="" commonoperatetype="" name="新建派发"/>
            <to-phase-id id="DraftTask" role="dealPerformer@" condition="${operateType}==" operatetype=""
                         workflowdisplay="false" isrejecttask="false" roleid="" commonoperatetype="" name="保存草稿"/>
        </phase-id>
        <phase-id id="DraftTask" name="草稿" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="ImplementDealTask" role="dealPerformer@" condition="${operateType}==" operatetype=""
                         workflowdisplay="false" isrejecttask="false" roleid="" commonoperatetype="" name="草稿派发"/>
        </phase-id>
        <phase-id id="RejectTask" name="驳回" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="ImplementDealTask" role="dealPerformer@" condition="${operateType}==" operatetype=""
                         workflowdisplay="false" isrejecttask="false" roleid="" commonoperatetype="" name="重新派发"/>
        </phase-id>
        <phase-id id="HoldTask" name="归档" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false"/>
        <phase-id id="ImplementDealTask" name="定单审核" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="ProjectDealTask" role="dealPerformer@" condition="${operateType}==93" operatetype=""
                         workflowdisplay="false" isrejecttask="false" roleid="" commonoperatetype="" name="送施工"/>
            <to-phase-id id="RejectTask" role="dealPerformer@" condition="${operateType}==0" operatetype=""
                         workflowdisplay="false" isrejecttask="false" roleid="" commonoperatetype="" name="驳回"/>
        </phase-id>
        <phase-id id="ProjectDealTask" name="最后一公里施工" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="ModifyDesignTask" role="dealPerformer@" condition="" operatetype="" workflowdisplay="false"
                         isrejecttask="false" roleid="" commonoperatetype="" name="送修改施工"/>
            <to-phase-id id="FDDITask" role="dealPerformer@" condition="" operatetype="" workflowdisplay="false"
                         isrejecttask="false" roleid="" commonoperatetype="" name="送光纤调度"/>
        </phase-id>
        <phase-id id="ModifyDesignTask" name="修改设计方案" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="AuditDesignTask" role="dealPerformer@" condition="" operatetype="" workflowdisplay="false"
                         isrejecttask="false" roleid="" commonoperatetype="" name="送审核方案"/>
        </phase-id>
        <phase-id id="AuditDesignTask" name="审核设计方案" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="ProjectDealTask" role="dealPerformer@" condition="" operatetype="" workflowdisplay="false"
                         isrejecttask="false" roleid="" commonoperatetype="" name="送施工"/>
        </phase-id>
        <phase-id id="FDDITask" name="光纤调度" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="TrasitionTask" role="dealPerformer@" condition="" operatetype="" workflowdisplay="false"
                         isrejecttask="false" roleid="" commonoperatetype="" name="送调度"/>
            <to-phase-id id="CityNetTask" role="dealPerformer@" condition="" operatetype="" workflowdisplay="false"
                         isrejecttask="false" roleid="" commonoperatetype="" name="送城域网配置"/>
            <to-phase-id id="ApnTask" role="dealPerformer@" condition="" operatetype="" workflowdisplay="false"
                         isrejecttask="false" roleid="" commonoperatetype="" name="送APN配置"/>
            <to-phase-id id="GGSNTask" role="dealPerformer@" condition="" operatetype="" workflowdisplay="false"
                         isrejecttask="false" roleid="" commonoperatetype="" name="送GGSN配置"/>
            <to-phase-id id="HLRTask" role="dealPerformer@" condition="" operatetype="" workflowdisplay="false"
                         isrejecttask="false" roleid="" commonoperatetype="" name="送HLR数据制作"/>
        </phase-id>
        <phase-id id="TrasitionTask" name="传输电路调度" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="BusinessTestTask" role="dealPerformer@" condition="" operatetype="" workflowdisplay="false"
                         isrejecttask="false" roleid="" commonoperatetype="" name="送测试"/>
        </phase-id>
        <phase-id id="CityNetTask" name="城域网配置" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="TrasitionTask" role="dealPerformer@" condition="" operatetype="" workflowdisplay="false"
                         isrejecttask="false" roleid="" commonoperatetype="" name="送电路调度"/>
        </phase-id>
        <phase-id id="ApnTask" name="APN资源配置" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="BusinessTestTask" role="dealPerformer@" condition="" operatetype="" workflowdisplay="false"
                         isrejecttask="false" roleid="" commonoperatetype="" name="送测试"/>
        </phase-id>
        <phase-id id="GGSNTask" name="GGSN配置" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="BusinessTestTask" role="dealPerformer@" condition="" operatetype="" workflowdisplay="false"
                         isrejecttask="false" roleid="" commonoperatetype="" name="送测试"/>
        </phase-id>
        <phase-id id="HLRTask" name="HLR数据制作" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="BusinessTestTask" role="dealPerformer@" condition="" operatetype="" workflowdisplay="false"
                         isrejecttask="false" roleid="" commonoperatetype="" name="送测试"/>
        </phase-id>
        <phase-id id="BusinessTestTask" name="网络端到端测试" type="operate" displaytransferaudit="false"
                  displayconfirm="false" displayreject="false" displaycommonoperate="false">
            <to-phase-id id="DredgeAffirmTask" role="dealPerformer@" condition="" operatetype="" workflowdisplay="false"
                         isrejecttask="false" roleid="" commonoperatetype="" name="送开通确认"/>
        </phase-id>
        <phase-id id="DredgeAffirmTask" name="开通确认" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="HoldTask" role="dealPerformer@" condition="" operatetype="" workflowdisplay="false"
                         isrejecttask="false" roleid="" commonoperatetype="" name="送归档"/>
        </phase-id>
    </flow-define>
</WorkFlowRule>

