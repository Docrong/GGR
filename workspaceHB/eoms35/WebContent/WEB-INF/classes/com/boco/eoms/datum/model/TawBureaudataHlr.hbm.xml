<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.boco.eoms.datum.model">
	<class
		name="TawBureaudataHlr"
		table="TAW_BUREAUDATA_HLR"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="id"
			type="integer" 
			column="id"
		>
			<generator class="sequence">
			    <param name="sequence">TAW_BUREAUDATA_HLR_SEQ</param>   
            </generator>
		</id>
		
		
		<property
			name="hlrid"
			column="HLRID"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="hlrname"
			column="HLRNAME"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="hlrsignalid"
			column="HLRSIGNALID"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="belongcityid"
			column="BELONGCITYID"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="bureauid"
			column="BUREAUID"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="remark"
			column="REMARK"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="deleted"
			column="DELETED"
			type="integer"
			not-null="false"
			length="22"
		/>
		
		<property name="sheetKey" />
		
		<property name="isOccupation" />


	</class>	
</hibernate-mapping>