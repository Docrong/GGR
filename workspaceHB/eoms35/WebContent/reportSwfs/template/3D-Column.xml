<?xml version="1.0" encoding="utf-8"?>
<root>
  <type> 
    <system> 
      <refresh enabled="yes"/> 
    </system>  
    <chart type="3DColumn" minimum_value="0.0" maximum_value="1.092"> 
      <animation show="yes"  speed="7"/>  
	  <names show="yes"  placement="chart" position="bottom">
         <font type="verdana_embed_tf" size="12" align="center"/>
	  </names>
      <column_chart column_space="1" round_radius="5"/> 
      <hints enabled="no" text="{BLOCK_NAME}, {NAME}" auto_size="yes"> 
         <background color="White"/> 
      </hints>	  
      <values show="yes" decimal_places="0">
        <font size="10"/>
      </values>
    </chart>  
	<workspace>
	   <chart_area width="500" height="300" x="40" y="20" deep="0">
		 <border enabled="yes" color="0xB54001"/>
	   </chart_area>
	   <base_area enabled="no"/>
	   <name text=" "/>
	   <grid>
		 <values>
			<lines color="0xB9B9B8"/>
		        <captions>
					<font size="12" type="arial"/>
				</captions>
				<ticks>
					<major_ticks>
						<lines length="5" color="0xFBF6EF" type="outside"/>
					</major_ticks>
				</ticks>
			</values>
		</grid>              
	   <x_axis smart="no"/>
	   <y_axis smart="yes"/>
	</workspace> 
  </type>
</root>