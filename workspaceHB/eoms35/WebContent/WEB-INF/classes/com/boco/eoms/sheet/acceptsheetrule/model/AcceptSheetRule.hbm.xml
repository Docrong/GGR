                unique="true"
        >

        </property>

	<property
            name="equipmentFactory"
            	column="equipmentFactory"
                length="100"
                not-null="false"
                unique="true"
        >

        </property>

	<property
            name="dealHuman"
            	column="dealHuman"
                length="100"
                not-null="true"
                unique="true"
        >
     </property>   
<property
            name="deleted"
            	column="deleted"
                length="16"
                not-null="false"
              
