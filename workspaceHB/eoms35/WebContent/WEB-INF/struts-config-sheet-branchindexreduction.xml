<struts-config>
    <form-beans>
        <form-bean
                name="SubtractTableForm"
                type="com.boco.eoms.sheet.branchindexreduction.webapp.form.SubtractTableForm"/>
        <form-bean name="FileUploadForm"
                   type="com.boco.eoms.sheet.branchindexreduction.webapp.form.FileUploadForm"/>

        <form-bean name="multiPageForm"
                   type="com.boco.eoms.sheet.base.webapp.form.MultiPageForm"/>
    </form-beans>
    <!-- ========== Data Sources Definitions =================================== -->
    <!--
        Define your Struts data sources in a file called struts-data-sources.xml and place
        it in your merge directory.
    -->

    <!-- ========== Form Bean Definitions =================================== -->
    <form-beans>
        <form-bean name="multiPageForm"
                   type="com.boco.eoms.sheet.base.webapp.form.MultiPageForm"/>
        <!--bean for EsmProcess end-->
    </form-beans>

    <!-- ========== Global Exceptions Definitions =================================== -->
    <global-exceptions>
        <exception type="java.lang.Exception" key="errors.general"
                   handler="com.boco.eoms.base.webapp.action.ActionExceptionHandler"/>
    </global-exceptions>

    <!-- ========== Global Forward Definitions =================================== -->
    <global-forwards>
        <forward name="selectFile" path="/selectFile.do"/>
        <forward name="logout" path="/logout.jsp"/>
        <forward name="error"
                 path="/WEB-INF/pages/wfworksheet/common/sheet_fail.jsp"
                 contextRelative="true"/>
    </global-forwards>

    <!-- ========== Action Mapping Definitions =================================== -->


    <action-mappings>
        <action
                path="/subtracttablesupload"
                type="com.boco.eoms.sheet.branchindexreduction.webapp.action.SubtractTableUploadAction"
                name="FileUploadForm"
                scope="request"
                input="mainMenu"
                parameter="method"
                unknown="false"
                validate="false">
            <set-property
                    property="cancellable"
                    value="true"/>
            <forward
                    name="edit"
                    path="/WEB-INF/pages/wfworksheet/branchindexreduction/subtractTableForm.jsp"
                    redirect="false"
                    contextRelative="true"/>
            <forward
                    name="list"
                    path="/WEB-INF/pages/wfworksheet/branchindexreduction/subtractTableSearch.jsp"
                    redirect="false"
                    contextRelative="true"/>
            <forward
                    name="detail"
                    path="/WEB-INF/pages/wfworksheet/branchindexreduction/subtractTableDetail.jsp"
                    redirect="false"
                    contextRelative="true"/>
        </action>
    </action-mappings>

