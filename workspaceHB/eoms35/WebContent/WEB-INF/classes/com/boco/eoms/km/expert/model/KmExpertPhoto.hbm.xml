<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.boco.eoms.km.expert.model.KmExpertPhoto"
		table="KM_EXPERT_PHOTO">

		<id name="id" column="id" unsaved-value="null">
			<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
			<generator class="uuid.hex">
			</generator>
		</id>

		<property name="createUser" column="CREATE_USER" length="32"
			not-null="true" unique="false">
		</property>
		
		<property name="createTime" column="CREATE_TIME" length="32"
			not-null="true" unique="false">
		</property>

		<property name="userId" column="USER_ID" length="32"
			not-null="true" unique="false">
		</property>

		<property name="photo" column="PHOTO" length="32"
			not-null="true" unique="false">
		</property>
	</class>
</hibernate-mapping>

