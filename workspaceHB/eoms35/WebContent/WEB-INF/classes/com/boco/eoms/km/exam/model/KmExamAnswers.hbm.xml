<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.exam.model.KmExamAnswers"
            table="KM_EXAM_ANSWERS"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="questionId"
            	column="QUESTION_ID"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="answer"
            	column="ANSWER"
                length="100"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="isRight"
            	column="IS_RIGHT"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="score"
            	column="SCORE"
                length="32"
                unique="false"
        >

        </property>
        
        
    <property
            name="testID"
            	column="TEST_ID"
                length="32"
                not-null="true"
                unique="true"
        >

        </property>

	<property
            name="attendUser"
            	column="ATTEND_USER"
                length="32"
                unique="false"
        >

        </property>

	<property
            name="referenceAnswer"
            	column="REFERENCE_ANSWER"
                length="32"
                unique="false"
        >

        </property>
	

	<property
            name="referenceScore"
            	column="REFERENCE_SCORE"
                length="32"
                unique="false"
        >

        </property>

    </class>    

</hibernate-mapping>

