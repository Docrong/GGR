		 <forward name="adminlist"
			path="/WEB-INF/pages/wfworksheet/urgentfault/adminlist.jsp" redirect="false"
			contextRelative="true" />
		 <forward name="event"
			path="/WEB-INF/pages/wfworksheet/urgentfault/event.jsp" redirect="false"
			contextRelative="true" />
