                <entry key="commonfault-onetimepass-KPI2_oracle">
                    <value>
                        classpath:config/statistic/commonfault-config/oracle/statistic-config-excel-commonfault-onetimepass-KPI2_oracle.xls
                    </value>
                </entry>

                <entry key="commonfault_intime_KPI1_oracle">
                    <value>
                        classpath:config/statistic/commonfault-config/oracle/statistic-config-excel-commonfault_intime_KPI1_oracle.xls
                    </value>
                </entry>

                <!--informix-->
                <entry key="commonfault-std_informix">
                    <value>
                        classpath:config/statistic/commonfault-config/informix/statistic-config-excel-commonfault-std_informix.xls
