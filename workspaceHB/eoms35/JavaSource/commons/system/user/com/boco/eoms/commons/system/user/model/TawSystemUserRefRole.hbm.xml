<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class
		name="com.boco.eoms.commons.system.user.model.TawSystemUserRefRole"
		table="taw_system_userrefrole">

		<id name="id" column="id" unsaved-value="null">

			<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
			<generator class="uuid.hex">

			</generator>

		</id>

		<property name="roleid" length="100">

		</property>

		<property name="userid" length="100">

		</property>

		<property name="subRoleid" length="100">

		</property>

		<property name="status" length="100">

		</property>

		<property name="remark" length="100">

		</property>

		<property name="currentsheetcount" length="200">

		</property>

		<property name="groupType" length="10">

		</property>

		<property name="roleType" length="10">

		</property>
		<property name="version" length="50" not-null="false" />
	</class>

</hibernate-mapping>

