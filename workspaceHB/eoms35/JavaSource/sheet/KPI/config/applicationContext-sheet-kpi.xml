<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
        "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

    <bean id="iKpiBaseDAO"
          class="com.boco.eoms.sheet.kpi.dao.hibernate.KpiBaseDAO"
          autowire="byName">
    </bean>
    <bean id="iKpiBaseManager" parent="txProxyTemplate">
        <property name="target">
            <bean
                    class="com.boco.eoms.sheet.kpi.service.impl.KpiBaseManagerImpl">
                <property name="kpiBaseDAO">
                    <ref bean="iKpiBaseDAO"/>
                </property>
            </bean>
        </property>
    </bean>


</beans>
