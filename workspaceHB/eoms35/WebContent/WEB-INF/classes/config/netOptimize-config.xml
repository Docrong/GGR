<?xml version="1.0" encoding="UTF-8"?>
<WorkFlowRule name="NetOptimize" description="网络优化流程">
   <flow-define id="NetOptimizeProcess" description="网络优化工单">
     <phase-id id="receive" type="receive">
       <to-phase-id id="AcceptTask" role="dealPerformer@361" condition="${operateType}==0" name="派发"/>
       <to-phase-id id="draftTask" role="" condition="${operateType}==22" name="保存草稿"/>   
     </phase-id>
     <phase-id id="draftTask" name="草稿" type="operate">   
        <to-phase-id id="AcceptTask" role="dealPerformer@361" condition="${operateType}==3" name="派发"/>
     </phase-id> 
     <phase-id id="AcceptTask" name="需求受理中" type="operate">  
        <to-phase-id id="ProjectCreateTask" role="dealPerformer@364" condition="${operateType}==46" name="制作方案"/>
        <to-phase-id id="draftTask" role="" condition="${operateType}==67" name="无需实施"/>
        <to-phase-id id="ExcuteTask" role="dealPerformer@363" condition="${operateType}==1" name="直接实施"/>
     </phase-id>     
     <phase-id id="ProjectCreateTask" name="方案制定中" type="operate">
        <to-phase-id id="AuditTask" role="dealPerformer@362" condition="${operateType}==2" name="制定方案，提交审核"/>
     </phase-id>
     <phase-id id="AuditTask" name="审批中" type="operate">
     	<to-phase-id id="ExcuteTask" role="dealPerformer@363" condition="${operateType}==66" name="审批通过"/>
     	<to-phase-id id="ProjectCreateTask" role="dealPerformer@364" condition="${operateType}==5" name="审批不通过"/>
     </phase-id>
     <phase-id id="ExcuteTask" name="实施中" type="operate">
     	<to-phase-id id="ResultEvaluateTask" role="dealPerformer@361" condition="${operateType}==62" name="完成配置，反馈实施结果"/>
     </phase-id>
     <phase-id id="ResultEvaluateTask" name="实施结果评估中" type="operate">
     	<to-phase-id id="HoldTask" role="" condition="${operateType}==18" name="验证实施结果，提交评定结果"/>
     </phase-id>
     <phase-id id="HoldTask" name="待归档" type="operate">      
     </phase-id>
     </flow-define>
</WorkFlowRule>
