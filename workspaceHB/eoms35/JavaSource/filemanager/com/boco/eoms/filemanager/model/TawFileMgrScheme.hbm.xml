<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.boco.eoms.filemanager.model.TawFileMgrScheme"
		table="taw_file_mgr_scheme">

		<id name="file_mgr_scheme_id" column="file_mgr_scheme_id" unsaved-value="null">

			<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
			<generator class="increment">

			</generator>

		</id>

		<property name="scheme_cycle_type" length="10">

		</property>

		<property name="scheme_cycle" length="255">

		</property>

		<property name="scheme_description" length="255">

		</property>

		<property name="scheme_ahead" length="10">

		</property>

		<property name="scheme_time" length="255">

		</property>

		<property name="topic_id" length="10">

		</property>

		<property name="title" length="255">

		</property>

		<property name="report_description" length="1000">

		</property>

		<property name="fault_class" length="10">

		</property>
		
		<property name="send_dept_id" length="255">
		
		</property>
		
		<property name="send_contact" length="100">
		
		</property>
		
		<property name="send_dept_name" length="255">
		
		</property>
		
		<property name="create_user_id" length="255">
		
		</property>
		
		<property name="accept_dept_id" length="1000">
		
		</property>
		
		<property name="accept_dept_name" length="2000">
		
		</property>
		
		<property name="specialty_id" length="255">
		
		</property>
		
		<property name="combintype" length="1">
		
		</property>
		
		<property name="audit_user_id" length="1000">
		
		</property>
		
		<property name="report_user_id" length="1000">
		
		</property>
		
		<property name="audit_user_name" length="1000">
		
		</property>
		
		<property name="report_user_name" length="1000">
		
		</property>
		
		<property name="is_audit" length="1">
		
		</property>
		

	</class>

</hibernate-mapping>

