<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.eva.model.EvaKpi"
            table="eva_kpi"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

		<property
            name="kpiName"
            	column="kpi_name"
                length="255"
                not-null="false"
                unique="false"
        >

        </property>
        
        <property
            name="cycle"
            	column="cycle"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
        <property
            name="threshold"
            	column="threshold"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

        <property
            name="ruleGroupId"
            	column="rule_group_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
        <property
            name="creator"
            	column="creator"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
        <property
            name="createTime"
            	column="create_time"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
        <property
            name="remark"
            	column="remark"
                length="255"
                not-null="false"
                unique="false"
        >

        </property>
        
        <property
            name="deleted"
            	column="deleted"
                length="32"
                not-null="false"
                unique="false"
        >

        </property> 
        
        <property
            name="algorithm"
            	column="algorithm"
                length="255"
                not-null="false"
                unique="false"
        >

        </property>
        
    </class>    

</hibernate-mapping>

