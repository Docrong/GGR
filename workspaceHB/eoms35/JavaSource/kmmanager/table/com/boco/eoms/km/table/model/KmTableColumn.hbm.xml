<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.boco.eoms.km.table.model.KmTableColumn"
		table="KM_TABLE_COLUMN">

		<id name="id" column="id" unsaved-value="null">
			<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
			<generator class="uuid.hex">
			</generator>
		</id>

		<property name="tableId" column="TABLE_ID" length="32" not-null="true" unique="false">
		</property>

		<property name="createUser" column="CREATE_USER" length="32" not-null="true" unique="false">
		</property>

		<property name="createDept" column="CREATE_DEPT" length="32" not-null="true" unique="false">
		</property>

		<property name="createTime" column="CREATE_TIME" length="32" not-null="true" unique="false">
		</property>

		<property name="colName" column="COL_NAME" length="32" not-null="true" unique="false">
		</property>

		<property name="colChname" column="COL_CHNAME" length="32" not-null="true" unique="false">
		</property>

		<property name="colType" column="COL_TYPE" length="32" not-null="true" unique="false">
		</property>

		<property name="colDictType" column="COL_DICT_TYPE" length="32" not-null="false" unique="false">
		</property>

		<property name="colDictId" column="COL_DICT_ID" length="32" not-null="false" unique="false">
		</property>

		<property name="colDefault" column="COL_DEFAULT" length="32" not-null="false" unique="false">
		</property>

		<property name="colSize" column="COL_SIZE" length="32" not-null="true" unique="false">
		</property>

		<property name="isNullable" column="IS_NULLABLE" length="32" not-null="true" unique="false">
		</property>

		<property name="isOpen" column="IS_OPEN" length="32" not-null="false" unique="false">
		</property>

		<property name="isVisibl" column="IS_VISIBL" length="32" not-null="true" unique="false">
		</property>

		<property name="isUnique" column="IS_UNIQUE" length="32" not-null="true" unique="false">
		</property>

		<property name="isIndex" column="IS_INDEX" length="32" not-null="false" unique="false">
		</property>

		<property name="isDeleted" column="IS_DELETED" length="32" not-null="false" unique="false">
		</property>

		<property name="orderBy" column="ORDER_BY" length="32" not-null="false" unique="false">
		</property>
		
		<property name="mark" column="MARK" length="32" not-null="false" unique="false">
		</property>
		
		<property name="subNode" column="SUB_NODE" length="32" not-null="false" unique="false">
		</property>

		<property name="parentNode" column="PARENT_NODE" length="32" not-null="false" unique="false">
		</property>
	</class>

</hibernate-mapping>

