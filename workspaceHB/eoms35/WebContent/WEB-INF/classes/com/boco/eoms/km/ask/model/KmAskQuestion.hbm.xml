<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.ask.model.KmAskQuestion"
            table="KM_ASK_QUESTION"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="name"
            	column="NAME"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="question"
            	column="QUESTION"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="speciality"
            	column="SPECIALITY"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="answerCount"
            	column="ANSWER_COUNT"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="questionStatus"
            	column="QUESTION_STATUS"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="askUser"
            	column="ASK_USER"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="askDept"
            	column="ASK_DEPT"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="askTime"
            	column="ASK_TIME"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="score"
            	column="SCORE"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="questionSupply"
            	column="QUESTION_SUPPLY"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="userfulTime"
            	column="USERFUL_TIME"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="isAddScore"
            	column="IS_ADD_SCORE"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="isClosed"
            	column="IS_CLOSED"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="isAnonymity"
            	column="IS_ANONYMITY"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        
    </class>    

</hibernate-mapping>

