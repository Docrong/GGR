<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">

<struts-config>

	<!-- ========== Data Sources Definitions =================================== -->
	<!--
		Define your Struts data sources in a file called struts-data-sources.xml and place
		it in your merge directory.
	-->

	<!-- ========== Form Bean Definitions =================================== -->
	<form-beans>
		<form-bean name="multiPageForm"
			type="com.boco.eoms.datum.webapp.form.TawImportExcelForm" />
		<!--bean for EsmProcess end-->
	</form-beans>

	<!-- ========== Global Exceptions Definitions =================================== -->
	<global-exceptions>
		<exception type="java.lang.Exception" key="errors.general"
			handler="com.boco.eoms.base.webapp.action.ActionExceptionHandler" />
	</global-exceptions>

	<!-- ========== Global Forward Definitions =================================== -->
	<global-forwards>
		<forward name="selectFile" path="/selectFile.do" />
		<forward name="logout" path="/logout.jsp" />
		<forward name="error"
			path="/WEB-INF/pages/wfworksheet/common/sheet_fail.jsp"
			contextRelative="true" />
		<forward name="nopriv" path="/sheetPriv.jsp"
			contextRelative="true" />
	</global-forwards>

	<!-- ========== Action Mapping Definitions =================================== -->
	<action-mappings>


		<action path="/bureaudataHlr"
			type="com.boco.eoms.datum.webapp.action.TawBureaudataHlrAction"
			name="multiPageForm" scope="request" input="error" parameter="method"
			unknown="false" validate="false" attribute="TawBureaudataHlr">
			<set-property property="cancellable" value="true" />
			<forward name="add"
				path="/WEB-INF/pages/wfworksheet/datum/TawBureaudataHlr/manager/entry.jsp"
				redirect="false" contextRelative="true" />
			<forward name="edit"
				path="/WEB-INF/pages/wfworksheet/datum/TawBureaudataHlr/manager/entry2.jsp"
				redirect="false" contextRelative="true" />
			<forward name="searchDo"
				path="/WEB-INF/pages/wfworksheet/datum/TawBureaudataHlr/query/Search.jsp"
				redirect="false" contextRelative="true" />
			<forward name="search"
				path="/WEB-INF/pages/wfworksheet/datum/TawBureaudataHlr/query/SearchInit.jsp"
				redirect="false" contextRelative="true" />
			<forward name="seinit"
				path="/WEB-INF/pages/wfworksheet/datum/TawBureaudataHlr/query/seinit.jsp"
				redirect="false" contextRelative="true" />
			<forward name="seinitDo"
				path="/WEB-INF/pages/wfworksheet/common/successMessage.jsp"
				redirect="false" contextRelative="true" />
			<forward name="success"
				path="/sheet/datum/bureaudataHlr.do?method=searchDo"
				redirect="false" contextRelative="true" />
			<forward name="postexcelfile"
				path="/WEB-INF/pages/wfworksheet/datum/importexcel/importexcel.jsp"
				redirect="false" contextRelative="true" />
			<forward name="exportexcelfile"
				path="/WEB-INF/pages/wfworksheet/datum/importexcel/export.jsp"
				redirect="false" contextRelative="true" />
			<forward name="report"
				path="/WEB-INF/pages/wfworksheet/datum/importexcel/report.jsp"
				redirect="false" contextRelative="true" />
			<forward name="nostandard"
				path="/WEB-INF/pages/wfworksheet/datum/importexcel/error.jsp"
				redirect="false" contextRelative="true" />
			<forward name="segnewexport"
				path="/WEB-INF/pages/wfworksheet/datum/importexcel/segNewExport.jsp"
				redirect="false" contextRelative="true" />
			<forward name="cityadjustexport"
				path="/WEB-INF/pages/wfworksheet/datum/importexcel/cityAdjustExport.jsp"
				redirect="false" contextRelative="true" />
			<forward name="changingexport"
				path="/WEB-INF/pages/wfworksheet/datum/importexcel/changingExport.jsp"
				redirect="false" contextRelative="true" />
			<forward name="wholeexport"
				path="/WEB-INF/pages/wfworksheet/datum/importexcel/wholeExport.jsp"
				redirect="false" contextRelative="true" />
			<forward name="fileAttachment"
				path="/WEB-INF/pages/wfworksheet/datum/standardtemp.zip"
				redirect="false" contextRelative="true" />
		</action>

		<action path="/bureaudataSegmenthlr"
			type="com.boco.eoms.datum.webapp.action.TawBureaudataSegmenthlrAction"
			name="multiPageForm" scope="request" input="error" parameter="method"
			unknown="false" validate="false"
			attribute="TawBureaudataSegmenthlr">
			<set-property property="cancellable" value="true" />
			<forward name="search"
				path="/WEB-INF/pages/wfworksheet/datum/TawBureaudataSegmenthlr/query/SearchInit.jsp"
				redirect="false" contextRelative="true" />
			<forward name="success"
				path="/WEB-INF/pages/wfworksheet/datum/TawBureaudataSegmenthlr/query/Search.jsp"
				redirect="false" contextRelative="true" />
		</action>
		
		<action path="/bureaudataCityzone"
			type="com.boco.eoms.datum.webapp.action.TawBureaudataCityzoneAction"
			name="multiPageForm" scope="request" input="error" parameter="method"
			unknown="false" validate="false"
			attribute="TawBureaudataSegmenthlr">
			<set-property property="cancellable" value="true" />
			<forward name="list"
				path="/WEB-INF/pages/wfworksheet/datum/TawBureaudataCityzone/manager/list.jsp"
				redirect="false" contextRelative="true" />
			<forward name="findByid"
				path="/WEB-INF/pages/wfworksheet/datum/TawBureaudataCityzone/manager/view.jsp"
				redirect="false" contextRelative="true" />
			<forward name="success"
				path="/sheet/datum/bureaudataCityzone.do?method=seach"
				redirect="false" contextRelative="true" />
		</action>

	</action-mappings>

	<controller nocache="true" inputForward="true" maxFileSize="2M" />
	<message-resources parameter="config/ApplicationResources-sheet"
		key="sheet" />
	<message-resources
		parameter="config/ApplicationResources-sheet-complaint"
		key="complaint" />
	<!-- ========== Validator plugin ===================  -->
	<plug-in className="org.apache.struts.validator.ValidatorPlugIn">
		<set-property property="pathnames"
			value="/WEB-INF/validator-rules.xml,
                             /WEB-INF/validator-rules-custom.xml,
                             /WEB-INF/validation.xml" />
	</plug-in>

</struts-config>