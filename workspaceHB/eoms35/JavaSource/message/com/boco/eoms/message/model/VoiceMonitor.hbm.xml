<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.message.model.VoiceMonitor"
            table="voice_monitor"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

        <property
            name="applyId"
                length="32"
                not-null="false"
                unique="false"
                column="apply_id"
        >

        </property>
        
        <property
            name="serviceId"
                length="32"
                not-null="false"
                unique="false"
                column="service_id"
        >

        </property>
        
        <property
            name="buizid"
                length="50"
                not-null="true"
                unique="false"
                column="buiz_id"
        >

        </property>

        <property
            name="content"
                length="500"
                not-null="false"
                unique="false"
        >

        </property>

        <property
            name="dispatchTime"
                length="30"
                not-null="false"
                unique="false"
                column="dispatch_time"
        >

        </property>
        
        <property
            name="allocTime"
                length="30"
                not-null="true"
                unique="false"
                column="alloc_time"
        >

        </property>
        
        <property
            name="finishTime"
                length="30"
                not-null="false"
                unique="false"
                column="finish_time"
        >

        </property>

        <property
            name="dutyman"
                length="30"
                not-null="false"
                unique="false"
        >

        </property>
        
        <property
            name="dutymanNum"
                length="36"
                not-null="false"
                unique="false"
                column="dutyman_num"
        >

        </property>

        <property
            name="receiverId"
                length="30"
                not-null="false"
                unique="false"
                column="receiver_id"
        >

        </property>
        
        <property
            name="receiverNum"
                length="36"
                not-null="false"
                unique="false"
                column="receiver_num"
        >

        </property>
        
        <property
            name="senderId"
                length="30"
                not-null="false"
                unique="false"
                column="sender_id"
        >

        </property>
        
        <property
            name="senderNum"
                length="36"
                not-null="false"
                unique="false"
                column="sender_num"
        >

        </property>        
                
        <property
            name="deleted"
                length="5"
                not-null="false"
                unique="false"
                column="deleted"
        >

        </property>

    </class>    

</hibernate-mapping>

