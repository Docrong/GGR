<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.commons.accessories.model.TawCommonsAccessoriesConfig"
            table="taw_commons_accessoriesconfig"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">
    </generator>

    </id>
       <property
            name="appId"
                length="50"
        >

        </property>
        <property
            name="allowFileType"
                length="50"
        >

        </property>

        <property
            name="appCode"
                length="50"
        >

        </property>

        <property
            name="appName"
                length="50"
        >

        </property>

        <property
            name="maxSize"
                length="50"
        >

        </property>

        <property
            name="path"
                length="50"
        >

        </property>

    </class>    

</hibernate-mapping>

