<?xml version="1.0" encoding="UTF-8"?>
<WorkFlowRule name="GroupCheck" description="集客投诉核查流程">
    <flow-define id="GroupCheck" description="集客投诉核查流程" tasknamespace="http://GroupCheck" linkservicename=""
                 dictfilename="dict-sheet-groupcheck" name="GroupCheck">
        <phase-id id="receive" name="新建工单" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="DraftTask" role="" condition="${operateType}==22" operatetype="" stepdisplay="false"
                         workflowdisplay="false" isrejecttask="false" roleid="" commonoperatetype="" name="保存草稿"/>
            <to-phase-id id="CityCheck" role="" condition="${operateType}==0" operatetype="" stepdisplay="false"
                         workflowdisplay="false" isrejecttask="false" roleid="" commonoperatetype="" name="自动转派"/>
        </phase-id>
        <phase-id id="DraftTask" name="草稿" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="CityCheck" role="" condition="${operateType}==3" operatetype="" stepdisplay="false"
                         workflowdisplay="false" isrejecttask="false" roleid="" commonoperatetype="" name="草稿派发"/>
        </phase-id>
        <phase-id id="HoldTask" name="自动归档" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false"/>
        <phase-id id="CityCheck" name="核查" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="HoldTask" role="" condition="${operateType}==101" operatetype="" stepdisplay="false"
                         workflowdisplay="false" isrejecttask="false" roleid="" commonoperatetype="" name="完成核查"/>
        </phase-id>
    </flow-define>
</WorkFlowRule>

