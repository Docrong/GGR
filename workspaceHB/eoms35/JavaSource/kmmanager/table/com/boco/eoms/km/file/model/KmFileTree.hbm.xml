<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.file.model.KmFileTree"
            table="KM_FILE_TREE"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="nodeName"
            	column="NODENAME"
                length="100"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="userId"
            	column="USERID"
                length="100"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="createTime"
            	column="CREATETIME"
                length="7"
                not-null="false"
                unique="false"
        >

        </property>

		<property
            name="nodeId"
            	column="node_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
        <property
            name="parentNodeId"
            	column="parent_node_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
        <property
            name="leaf"
            	column="leaf"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
              
        <property
            name="hasParentOperate"
            	column="has_parent_operate"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
    </class>    

</hibernate-mapping>

