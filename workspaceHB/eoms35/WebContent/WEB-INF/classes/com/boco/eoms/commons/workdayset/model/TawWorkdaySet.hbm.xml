        >

        </property>

        <property
            name="createTime"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
            name="deleted"
                length="5"
                not-null="false"
        >

        </property>

        <property
            name="endTime"
                length="10"
                not-null="false"
        >

        </property>

        <property
            name="startTime"
                length="10"
                not-null="false"
        >

        </property>

        <property
            name="status"
                length="5"
                not-null="false"
        >

        </property>
        <property
            name="cover"
                length="5"
                not-null="false"
        >

        </property>
        <property
            name="userId"
                length="30"
                not-null="false"
        >

        </property>

        <property
            name="workDate"
                length="30"
                not-null="false"
        >

        </property>

    </class>    

</hibernate-mapping>

