                <entry key="commonfault_T_resolve_KPI5_informix">
                    <value>
                        classpath:config/statistic/commonfault-config/informix/statistic-config-excel-commonfault_T_resolve_KPI5_informix.xls
                    </value>
                </entry>
                <entry key="commonfault_T_resolve_KPI6_informix">
                    <value>
                        classpath:config/statistic/commonfault-config/informix/statistic-config-excel-commonfault_T_resolve_KPI6_informix.xls
