<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.boco.eoms.datum.model">
	<class
		name="TawBureaudataCityzone"
		table="TAW_BUREAUDATA_CITYZONE"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="cityid"
			type="integer"
			column="cityid"
		>
			<generator class="sequence"/>
		</id>

		<property
			name="cityname"
			column="CITYNAME"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="zonenum"
			column="ZONENUM"
			type="string"
			not-null="true"
			length="5"
		/>
		<property
			name="deptid"
			column="DEPTID"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="usersysid"
			column="USERSYSID"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="userid"
			column="USERID"
			type="string"
			not-null="false"
			length="50"
		/>


	</class>	
</hibernate-mapping>