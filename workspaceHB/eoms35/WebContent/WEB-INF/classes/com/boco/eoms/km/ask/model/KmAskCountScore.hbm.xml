<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.ask.model.KmAskCountScore"
            table="KM_ASK_COUNT_SCORE"
    >

        <id
                name="id"
                column="id"
                unsaved-value="null"
        >

            <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
            <generator class="uuid.hex">

            </generator>

        </id>

        <property
                name="userName"
                column="USER_NAME"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="userDept"
                column="USER_DEPT"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="countScore"
                column="COUNT_SCORE"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="role"
                column="ROLE"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>


    </class>

</hibernate-mapping>

