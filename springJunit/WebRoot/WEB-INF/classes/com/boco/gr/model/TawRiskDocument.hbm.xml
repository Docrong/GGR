<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.boco.gr.model.TawRiskDocument"
            table="taw_risk_document">

    <id name="id" column="id" unsaved-value="null">
    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">
    </generator>
    </id>
                                                                     
         <property name="deleted" column="deleted"  not-null="false" unique="false"></property>                                                                                    
         <property name="riskType" column="riskType"  not-null="false" unique="false"></property> 
         <property name="isShow" column="isShow"  not-null="false" unique="false"></property> 
         
         <property name="docTitle" column="docTitle"  not-null="false" unique="false"></property>                                                                                    
         <property name="docDesc" column="docDesc"  not-null="false" unique="false"></property>   
         <property name="docLinkName" column="docLinkName"  not-null="false" unique="false"></property>   
            
                                                                                          
         <property name="uploadTime" column="uploadTime"  not-null="false" unique="false"></property>                                                                                                                                                                        
         <property name="uploadUserId" column="uploadUserId"  not-null="false" unique="false"></property>                                                                                                                                                                    
         <property name="uploadUserDeptid" column="uploadUserDeptid"  not-null="false" unique="false"></property>                                                                                                                                                                      
         <property name="uploadUserPhone" column="uploadUserPhone"  not-null="false" unique="false"></property>    
         
         <property name="updateTime" column="updateTime"  not-null="false" unique="false"></property>                                                                                                                                                                        
         <property name="updateUserId" column="updateUserId"  not-null="false" unique="false"></property>                                                                                                                                                                    
         <property name="updateUserDeptid" column="updateUserDeptid"  not-null="false" unique="false"></property>                                                                                                                                                                      
         <property name="updateUserPhone" column="updateUserPhone"  not-null="false" unique="false"></property>  
         <property name="sysNo" column="sysNo"  not-null="false" unique="false"></property>  
    </class>    

</hibernate-mapping>

