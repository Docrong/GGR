<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.boco.eoms.sheet.autoCrm.model">
    <class
            name="Autot1Crm"
            table="AUTOT1_CRM"
    >
        <meta attribute="sync-DAO">false</meta>
        <id
                name="id"
                type="string"
                column="id"
        >
            <generator class="sequence"/>
        </id>

        <property
                name="faultsite"
                column="FAULTSITE"
                type="string"
                not-null="false"
                length="50"
        />
        <property
                name="colseswitch"
                column="COLSESWITCH"
                type="string"
                not-null="false"
                length="5"
        />
        <property
                name="autoacceptrolet1"
                column="AUTOACCEPTROLET1"
                type="string"
                not-null="false"
                length="32"
        />
        <property
                name="dealrolet2"
                column="DEALROLET2"
                type="string"
                not-null="false"
                length="32"
        />
        <property
                name="remark1"
                column="REMARK1"
                type="string"
                not-null="false"
                length="50"
        />
        <property
                name="remark2"
                column="REMARK2"
                type="string"
                not-null="false"
        />
        <property
                name="createdate"
                column="CREATEDATE"
                type="date"
                not-null="false"
                length="7"
        />
        <property
                name="templatename"
                column="TEMPLATENAME"
                type="string"
                not-null="false"
                length="200"
        />


    </class>
</hibernate-mapping>