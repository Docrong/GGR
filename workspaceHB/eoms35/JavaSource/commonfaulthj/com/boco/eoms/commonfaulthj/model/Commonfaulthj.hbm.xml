<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.commonfaulthj.model.Commonfaulthj"
            table="commonfault_hj"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="mainid"
            	column="mainid"
                length="50"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="sheetid"
            	column="sheetid"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="creater"
            	column="creater"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="savetime"
            	column="savetime"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="updatetime"
            	column="updatetime"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="sendyear"
            	column="sendyear"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="sendmonth"
            	column="sendmonth"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="sendday"
            	column="sendday"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="remark"
            	column="remark"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>
        
     <property
            name="subtractType"
            	column="subtractType"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>


    </class>    

</hibernate-mapping>

