<?xml version="1.0" encoding="GB2312"?>
<boco>
	<!-- 派单接口 -->
	<newWorkSheet>
		<content columnName="sheetID"  interfaceEnName="SheetID" defauleValue="" interfaceCnName="工单ID"/>
		<content columnName="serialID" interfaceEnName="SerialID" defauleValue="" interfaceCnName="工单流水号" />
		<content columnName="operators" interfaceEnName="Operators" defauleValue="" interfaceCnName="工单处理人"/>
		<content columnName="attachRef" interfaceEnName="AttachRef" defauleValue="" interfaceCnName="附件文件名" />
		<content columnName="busiName" interfaceEnName="BusiName" defauleValue="" interfaceCnName="业务名称" />
		<content columnName="sheetMemo" interfaceEnName="sheetMemo" defauleValue="" interfaceCnName="工单主题" />
		<content columnName="startTime" interfaceEnName="startTime" defauleValue="" interfaceCnName="派单时间" />
		<content columnName="endTime" interfaceEnName="endTime" defauleValue="" interfaceCnName="截止时间" />
		<content columnName="subversion" interfaceEnName="subversion" defauleValue="" interfaceCnName="局数据文号" />
	</newWorkSheet>
	
	<!-- 状态同步接口 -->
	<syncJWorkSheet>
		<content columnName="preLinkId"  interfaceName="subTkid" defauleValue="" interfaceCnName="EOMS子流程id"/>
		<content columnName="operateUserName" interfaceName="opUserName" defauleValue="" interfaceCnName="操作人" />
		<content columnName="operateTime" interfaceName="opTime" defauleValue="" interfaceCnName="操作时间"/>
		<content columnName="sheetStatus" interfaceName="sheetStatus" defauleValue="" interfaceCnName="工单状态" dictValuePath="sheetStatus"/>
	</syncJWorkSheet>
	
	<!-- 激活接口 -->
	<activateSheet>	
		<content columnName="preLinkId"  interfaceName="subTkid" defauleValue="" interfaceCnName="EOMS子流程id"/>
		<content columnName="isCreated"  interfaceName="isCreated" defauleValue="" interfaceCnName="是否新建成功"/>
		<content columnName="backReason" interfaceName="backReason" defauleValue="" interfaceCnName="退回原因" />
	</activateSheet>
	
	<!-- 获取局数据下载文件配置 -->
	<getTemplatePath>
		<content columnName="BusiName" interfaceEnName="BusiName" defauleValue="" interfaceCnName="业务名称"/>
	</getTemplatePath>
	<uploadType>http</uploadType><!-- 附件上传方式 -->
	<interfaces>
		<url>http://10.30.244.20:8092/iDataMgr/webService/SheetHandle?wsdl</url><!-- 接口url地址 -->
		<timeout>60000</timeout><!-- 超时时间 -->
		<serSupplier>test</serSupplier><!-- 服务提供方 -->
		<serCaller>test</serCaller><!-- 服务调用方 -->
		<callerPwd>test</callerPwd><!-- 口令 -->
	</interfaces>
	
	<!-- 自动校验环节的虚拟账号的相关信息 -->
	<autoCheck>
		<userId>admin</userId>
		<subRoleId>8a528342374edae701375355230f0183</subRoleId>
		<deptId>11</deptId>
		<contact>110</contact>
	</autoCheck>
	
	
	
	
	
	<dict>
		<!-- 现场同事自行建立对应的字典值，父字典项：工单状态 字字典项：开始制作、制作中、制作完成、开始核查、核查中、核查完成、工单完成-->
		<sheetStatus>
			<item dictid="101400101" interfacecode="10101" /><!-- 网元选择  -->
			<item dictid="101400102" interfacecode="10102" /><!-- 核查完成  -->
			<item dictid="101400103" interfacecode="10103" /><!-- 脚本制作完成  -->
			<item dictid="101400104" interfacecode="10104" /><!-- 脚本下发完成  -->
			<item dictid="101400105" interfacecode="10105" /><!-- 数据拨测完成  -->
			<item dictid="101400106" interfacecode="10106" /><!-- 下发检查完成  -->
			<item dictid="101400107" interfacecode="10107" /><!-- 话单测试完成  -->
			<item dictid="101400108" interfacecode="10108" /><!-- 信令测试完成  -->
			<item dictid="101400109" interfacecode="10109" /><!-- 归档完成  -->
			<item dictid="101400110" interfacecode="10110" /><!-- 撤销工单  -->
		</sheetStatus>
	</dict>
	
	<!-- add by weichao 20161205 -->
	<majorDictId>1010104</majorDictId><!-- 专业的字典值 -->
	<userOrsubrole>0</userOrsubrole><!-- 新增工单的时候，制作人是选人还是选角色，0表示选人，1表示选子角色-->
</boco>
