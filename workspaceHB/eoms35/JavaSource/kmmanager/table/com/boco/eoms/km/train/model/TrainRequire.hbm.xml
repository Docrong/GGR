<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.train.model.TrainRequire"
            table="KM_TRAIN_REQUIRE"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="trainNo"
            	column="TRAIN_NO"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="trainTel"
            	column="TRAIN_TEL"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="trainQuestion"
            	column="TRAIN_QUESTION"
                length="100"
                not-null="true"
                unique="false"
        >

        </property>


	<property
            name="trainContent"
            	column="TRAIN_CONTENT"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="trainDept"
            	column="TRAIN_DEPT"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="trainVender"
            	column="TRAIN_VENDER"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="trainSpeciality"
            	column="TRAIN_SPECIALITY"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="trainType"
            	column="TRAIN_TYPE"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="trainUser"
            	column="TRAIN_USER"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="trainDate"
            	column="TRAIN_DATE"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="trainFile"
            	column="TRAIN_FILE"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="isDelete"
            	column="IS_DELETE"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
	
	
    </class>    

</hibernate-mapping>

