<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.sheet.tool.complaintmsgconfig.model.ComplaintSheetMsgConfig"
            table="complaint_sheet_msg_config"
    >

        <id
                name="id"
                column="id"
                unsaved-value="null"
        >

            <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
            <generator class="uuid.hex">

            </generator>

        </id>

        <property
                name="userId"
                column="user_id"
                length="50"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="areaId"
                column="area_id"
                length="50"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="complaintType"
                column="complaint_type"
                length="50"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="notifyUserIds"
                column="notify_user_ids"
                length="255"
                not-null="true"
                unique="false"
        >

        </property>


        <property
                name="accessoriesId"
                column="accessories_id"
                length="50"
                not-null="false"
                unique="false"
        >

        </property>

    </class>

</hibernate-mapping>

