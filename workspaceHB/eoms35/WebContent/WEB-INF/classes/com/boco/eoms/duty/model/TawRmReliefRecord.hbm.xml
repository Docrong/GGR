<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.duty.model.TawRmReliefRecord"
            table="taw_rm_reliefrecord"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

        <property
            name="handoverId"
            	column="handover_id"
                length="30"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="successorId"
            	column="successor_id"
                length="30"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="roomId"
            	column="room_id"
                length="30"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="time"
            	column="time"
                length="30"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="lbProblem"
            	column="lb_problem"
                length="100"
                not-null="false"
                unique="true"
        >

        </property>

        <property
            name="nuiteFlag"
            	column="nuite_flag"
                length="10"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="userId"
            	column="user_id"
                length="30"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="workSerial"
            	column="work_serial"
                length="30"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="createTime"
            	column="create_time"
                length="30"
                not-null="true"
                unique="true"
        >

        </property>
        
    </class>    

</hibernate-mapping>

