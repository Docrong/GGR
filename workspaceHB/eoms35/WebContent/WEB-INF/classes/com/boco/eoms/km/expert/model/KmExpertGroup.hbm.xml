<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.boco.eoms.km.expert.model.KmExpertGroup"
		table="KM_EXPERT_GROUP">

		<id name="id" column="ID" unsaved-value="null">
			<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
			<generator class="uuid.hex">
			</generator>
		</id>

		<property name="createUser" column="CREATE_USER" length="32"
			not-null="false" unique="false">
		</property>
		
		<property name="createTime" column="CREATE_TIME" length="32"
			not-null="false" unique="false">
		</property>

		<property name="groupName" column="GROUP_NAME" length="32"
			not-null="false" unique="false">
		</property>

		<property name="specialty" column="SPECIALTY" length="32"
			not-null="false" unique="false">
		</property>
		
		<property name="groupLeader" column="GROUP_LEADER" length="32"
			not-null="false" unique="false">
		</property>
	</class>

</hibernate-mapping>

