<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.exam.model.KmExamTest"
            table="KM_EXAM_TEST"
    >

        <id
                name="testID"
                column="TEST_ID"
                unsaved-value="null"
        >

            <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
            <generator class="uuid.hex">

            </generator>

        </id>


        <property
                name="testName"
                column="TEST_NAME"
                length="100"
                not-null="true"
                unique="false"
        >
        </property>

        <property
                name="testDescription"
                column="TEST_DESCRIPTION"
                length="500"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="createUser"
                column="CREATE_USER"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="createDept"
                column="CREATE_DEPT"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="createTime"
                column="CREATE_TIME"
                length="34"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="testBeginTime"
                column="TEST_BEGIN_TIME"
                length="34"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="testEndTime"
                column="TEST_END_TIME"
                length="34"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="testDuration"
                column="TEST_DURATION"
                length="20"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="totalScore"
                column="TOTAL_SCORE"
                length="20"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="isDeleted"
                column="IS_DELETED"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="specialtyID"
                column="SPECIALTY_ID"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="DeptID"
                column="DEPT_ID"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="isPublic"
                column="IS_PUBLIC"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="isPermittedOvertime"
                column="IS_PERMITTED_OVERTIME"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>


    </class>

</hibernate-mapping>

