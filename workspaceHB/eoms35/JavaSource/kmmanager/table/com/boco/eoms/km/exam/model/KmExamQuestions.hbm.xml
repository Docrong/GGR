<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
            name="com.boco.eoms.km.exam.model.KmExamQuestions"
            table="KM_EXAM_QUESTIONS"
    >

    <id
        name="questionID"
            column="QUESTION_ID"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	
	<property
            name="createUser"
            	column="CREATE_USER"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="createTime"
            	column="CREATE_TIME"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="createDept"
            	column="CREATE_DEPT"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="specialtyID"
            	column="SPECIALTY_ID"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="questionName"
            	column="QUESTION_NAME"
                length="50"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="question"
            	column="QUESTION"
                length="500"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="answer"
            	column="ANSWER"
                length="500"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="questionType"
            	column="QUESTION_TYPE"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="difficulty"
            	column="DIFFICULTY"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="isDeleted"
            	column="IS_DELETED"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="deptId"
            	column="DEPT_ID"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="accessory"
            	column="ACCESSORY"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
	
    </class>    

</hibernate-mapping>

