<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.commons.system.dict.model.TawSystemDictType"
            table="Taw_System_DictType"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="native">

    </generator>

    </id>

        <property
            name="dictId"
                length="100"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="dictCode"
                length="100"
                not-null="true"
        >

        </property>

        <property
            name="dictName"
                length="100"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="dictRemark"
                length="256"
        >

        </property>
         
        <property
            name="parentDictId"
                length="100"
        >

        </property>

        <property
            name="sysType"
                length="50"
        >

        </property>
        <property
            name="leaf"
                length="2"
        >

        </property>
    </class>    

</hibernate-mapping>

