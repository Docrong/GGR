    <controller nocache="true" inputForward="true" maxFileSize="2M"/>
    <message-resources parameter="config/ApplicationResources-sheet" key="sheet"/>
    <message-resources parameter="config/ApplicationResources-sheet-agentmaintenance" key="agentmaintenance"/>
    <!-- ========== Validator plugin ===================  -->
    <plug-in className="org.apache.struts.validator.ValidatorPlugIn">
        <set-property property="pathnames"
                      value="/WEB-INF/validator-rules.xml, /WEB-INF/validator-rules-custom.xml, /WEB-INF/validation.xml"/>
    </plug-in>

</struts-config>