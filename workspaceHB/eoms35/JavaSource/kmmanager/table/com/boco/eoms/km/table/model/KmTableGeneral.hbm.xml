<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.boco.eoms.km.table.model.KmTableGeneral"
		table="KM_TABLE_GENERAL">

		<id name="id" column="id" unsaved-value="null">

			<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
			<generator class="uuid.hex">

			</generator>

		</id>

		<property name="tableName" column="TABLE_NAME" length="32"
			not-null="true" unique="false">

		</property>

		<property name="tableChname" column="TABLE_CHNAME" length="32"
			not-null="true" unique="false">

		</property>

		<property name="createUser" column="CREATE_USER" length="32"
			not-null="true" unique="false">

		</property>

		<property name="createDept" column="CREATE_DEPT" length="32"
			not-null="true" unique="false">

		</property>

		<property name="createTime" column="CREATE_TIME" length="32"
			not-null="true" unique="false">

		</property>

		<property name="remark" column="REMARK" length="32"
			not-null="false" unique="false">

		</property>

		<property name="orderBy" column="ORDER_BY" length="32"
			not-null="false" unique="false">

		</property>

		<property name="isOpen" column="IS_OPEN" length="32"
			not-null="true" unique="false">

		</property>

		<property name="isVisibl" column="IS_VISIBL" length="32"
			not-null="true" unique="false">

		</property>

		<property name="isAudit" column="IS_AUDIT" length="32"
			not-null="true" unique="false">

		</property>

		<property name="isDeleted" column="IS_DELETED" length="32"
			not-null="false" unique="false">

		</property>
		
		<property name="isCreated" column="IS_CREATED" length="32"
			not-null="false" unique="false">
		</property>

		<property name="themeId" column="THEME_ID" length="32"
			not-null="true" unique="false">

		</property>


	</class>

</hibernate-mapping>

