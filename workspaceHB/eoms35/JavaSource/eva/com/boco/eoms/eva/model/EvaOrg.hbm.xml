<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
            name="com.boco.eoms.eva.model.EvaOrg"
            table="eva_org"
    >
    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>
		<property
            name="templateId"
            	column="template_id"
                length="255"
                not-null="false"
                unique="false"
        >
        </property>

		<property
            name="orgId"
            	column="org_id"
                length="255"
                not-null="false"
                unique="false"
        >
        </property>

		<property
            name="orgType"
            	column="org_type"
                length="255"
                not-null="false"
                unique="false"
        >
        </property>

		<property
            name="actionType"
            	column="action_type"
                length="255"
                not-null="false"
                unique="false"
        >
        </property>
		<property
            name="status"
            	column="status"
                length="255"
                not-null="false"
                unique="false"
        >
        </property>
		<property
            name="operateTime"
            	column="operate_time"
                length="255"
                not-null="false"
                unique="false"
        >
        </property>
		<property
            name="evaStartTime"
            	column="eva_start_time"
                length="255"
                not-null="false"
                unique="false"
        >
        </property>
		<property
            name="evaEndTime"
            	column="eva_end_time"
                length="255"
                not-null="false"
                unique="false"
        >
        </property>
    </class>    

</hibernate-mapping>
