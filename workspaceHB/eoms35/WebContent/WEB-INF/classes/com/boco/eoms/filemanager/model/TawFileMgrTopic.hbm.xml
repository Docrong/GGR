<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.boco.eoms.filemanager.model.TawFileMgrTopic"
		table="taw_file_mgr_topic">

		<id name="topic_id" column="topic_id" unsaved-value="null">

			<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
			<generator class="increment">

			</generator>

		</id>

		<property name="topic_type" length="10">

		</property>

		<property name="topic_type_name" length="255">

		</property>

		<property name="par_topic_id" length="10">

		</property>

		<property name="topic_name" length="32">

		</property>

		<property name="topic_memo" length="255">

		</property>

		<property name="topic_order" length="10">

		</property>

		<property name="topic_path" length="32">

		</property>

		<property name="class_id" length="5">

		</property>

		<property name="creat_time" length="19">

		</property>

	</class>

</hibernate-mapping>

