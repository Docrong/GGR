                    <value>
                        classpath:config/statistic/commonfault-config/oracle/statistic-config-query-commonfault_intime_KPI1_oracle.xml
                    </value>
                </entry>

                <!--informix-->
                <entry key="commonfault-std_informix">
                    <value>
                        classpath:config/statistic/commonfault-config/informix/statistic-config-query-commonfault-std_informix.xml
                    </value>
                </entry>
                <entry key="commonfault_delay_KPI3_informix">
                    <value>
                        classpath:config/statistic/commonfault-config/informix/statistic-config-query-commonfault_delay_KPI3_informix.xml
                    </value>
                </entry>
                <entry key="commonfault_T_resolve_KPI4_informix">
