<?xml version="1.0"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<!-- sample config file  , 
			create new config file in your module -->
<beans>
    <!-- 配置信息类 -->
    <bean id="statCommonfaultConfigManager"
          class="com.boco.eoms.commons.statistic.base.mgr.impl.StatConfigManagerImpl"
          autowire="byName">

        <!-- 是否每次都从新获取配置文件 true:时时都读取 false：相反-->
        <property name="reloadConfig">
            <value>true</value>
        </property>

        <!-- 配置Excel文件路径 key为url传入的参数，value对应文件名称  -->
        <property name="excelConfigMap">
            <map>


                <!--oracle-->
                <entry key="commonfault-std_oracle">
                    <value>
                        classpath:config/statistic/commonfault-config/oracle/statistic-config-excel-commonfault-std_oracle.xls
                    </value>
                </entry>
                <entry key="commonfault_delay_KPI3_oracle">
                    <value>
                        classpath:config/statistic/commonfault-config/oracle/statistic-config-excel-commonfault_delay_KPI3_oracle.xls
                    </value>
                </entry>
                <entry key="commonfault_T_resolve_KPI4_oracle">
                    <value>
                        classpath:config/statistic/commonfault-config/oracle/statistic-config-excel-commonfault_T_resolve_KPI4_oracle.xls
                    </value>
                </entry>
                <!-- 压力测试使用 不要删除 -->
                <entry key="commonfault_T_resolve_KPI4_oracle_test">
                    <value>
                        classpath:config/statistic/commonfault-config/oracle/statistic-config-excel-commonfault_T_resolve_KPI4_oracle_test.xls
                    </value>
                </entry>

                <entry key="commonfault_T_resolve_KPI5_oracle">
                    <value>
