<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.commons.system.user.model.TawSystemUser"
            table="taw_system_user"
    >

        <id
                name="id"
                column="id"
                unsaved-value="null"
        >

            <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
            <generator class="uuid.hex">

            </generator>

        </id>

        <property
                name="cptroomid"
                length="100"
        >

        </property>

        <property
                name="cptroomname"
                length="100"
        >

        </property>

        <property
                name="deptid"
                length="100"
        >

        </property>

        <property
                name="deptname"
                length="100"
        >

        </property>

        <property
                name="email"
                length="100"
        >

        </property>

        <property
                name="isPartners"
                length="100"
        >

        </property>

        <property
                name="familyaddress"
                length="100"
        >

        </property>

        <property
                name="fax"
                length="100"
        >

        </property>

        <property
                name="mobile"
                length="100"
        >

        </property>

        <property
                name="operuserid"
                length="100"
        >

        </property>

        <property
                name="phone"
                length="100"
        >

        </property>


        <property
                name="remark"
                length="100"
        >

        </property>

        <property
                name="sex"
                length="20"
        >

        </property>

        <property
                name="userdegree"
                length="100"
        >

        </property>

        <property
                name="userid"
                length="100"
        >

        </property>

        <property
                name="username"
                length="100"
        >

        </property>

        <property
                name="operremotip"
                length="100"
        >

        </property>

        <property
                name="savetime"

        >

        </property>

        <property
                name="updatetime"
                length="100"
        >

        </property>

        <property
                name="enabled"
                type="yes_no"
                column="account_enabled"
        >

        </property>

        <property
                name="accountExpired"
                type="yes_no"
                column="account_expired"
                not-null="true"
        >

        </property>

        <property
                name="accountLocked"
                type="yes_no"
                column="account_locked"
                not-null="true"
        >

        </property>

        <property
                name="credentialsExpired"
                type="yes_no"
                column="credentials_expired"
                not-null="true"
        >

        </property>

        <property
                name="password"
                column="password"
                not-null="true"
        >
        </property>
        <property
                name="deleted"
                column="deleted"
        >
        </property>
        <property
                name="isfullemploy"
                column="isfullemploy"
        >
        </property>
        <property
                name="isrest"
                column="isrest"
        >
        </property>
        <property
                name="portalrolename"
                column="portalrolename"
        >
        </property>
        <property
                name="userstatus"
                column="userstatus"
        >
        </property>


        <property
                name="attribute1"
                column="attribute1"
        >
        </property>
        <property
                name="attribute2"
                column="attribute2"
        >
        </property>
        <property name="failCount" column="fail_count"/>
    </class>

</hibernate-mapping>

