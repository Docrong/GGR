        <property name="completeFlag"/>
        <property name="completeTime"/>
        <property name="preLinkId"/>
        <property name="parentLinkId"/>
        <property name="firstLinkId"/>
        <property name="piid"/>
        <property name="aiid"/>
        <property name="activeTemplateId"/>
        <property name="nodeTemplateName"/>
        <property name="correlationKey"/>
        <property name="nodeAccessories"/>
        <property name="templateFlag"/>
        <property name="templateName"/>
        <property name="templateCreateUserId"/>
        <property name="transferReason"/>
        <property name="remark"/>
