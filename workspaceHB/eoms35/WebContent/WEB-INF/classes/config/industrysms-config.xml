                 dictfilename="dict-sheet-industrysms" name="IndustrySms">
        <phase-id id="receive" name="新建工单" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="DraftTask" role="" condition="${operateType}==22" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="保存草稿"/>
            <to-phase-id id="AuditHumTask" role="" condition="${operateType}==0" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="新建派发"/>
        </phase-id>
        <phase-id id="DraftTask" name="草稿" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false"/>
        <phase-id id="HoldTask" name="归档" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false"/>
        <phase-id id="AuditHumTask" name="审核" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="DataHumTask" role="" condition="${operateType}==102" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="审核通过"/>
        </phase-id>
        <phase-id id="DataHumTask" name="数据制作" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="HoldTask" role="" condition="${operateType}==103" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="数据制作完成"/>
        </phase-id>
    </flow-define>
</WorkFlowRule>

