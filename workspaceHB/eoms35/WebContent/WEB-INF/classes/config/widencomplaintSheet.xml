            select operateuserid,
            operatercontact,
            nodeacceptlimit,
            operatetime,
            activetemplateid,
            operateType as mainOperateType,
            remark,
            case
            when (case
            when operatetype = 61 then
            acceptflag
            else
            completeflag
            end) = 2 then
            'Y'
            else
            'N'
            end as limitFlag
            from widencomplaint_link
            where mainid = '@mainid@'
            order by operatetime
        </historySql>
