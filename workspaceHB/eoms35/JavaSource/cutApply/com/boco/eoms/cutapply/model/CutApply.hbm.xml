<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.cutapply.model.CutApply"
            table="cut_apply"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="userId"
            	column="user_id"
                length="32"
                not-null="true"
                unique="true"
        >

        </property>

	<property
            name="deptId"
            	column="dept_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="context"
            	column="context"
                length="255"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="areaId"
            	column="area_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="cutStartTime"
            	column="cut_start_time"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="cutEndTime"
            	column="cut_end_time"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="manager"
            	column="manager"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="phone"
            	column="phone"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="fileId"
            	column="file_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="isAffect"
            	column="is_affect"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="affectStartTime"
            	column="affect_start_time"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="affectEndTime"
            	column="affect_end_time"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="affectInfo"
            	column="affect_info"
                length="255"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="remark"
            	column="remark"
                length="255"
                not-null="false"
                unique="false"
        >

        </property>

        
    </class>    

</hibernate-mapping>

