<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.train.model.TrainInformation"
            table="KM_TRAIN_INFORMATION"
    >

        <id
                name="id"
                column="id"
                unsaved-value="null"
        >

            <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
            <generator class="uuid.hex">

            </generator>

        </id>

        <property
                name="trainName"
                column="TRAIN_NAME"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainGrade"
                column="TRAIN_GRADE"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainAddress"
                column="TRAIN_ADDRESS"
                length="100"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainTime"
                column="TRAIN_TIME"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainBeginTime"
                column="TRAIN_BEGIN_TIME"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainEndTime"
                column="TRAIN_END_TIME"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainDept"
                column="TRAIN_DEPT"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainDocument"
                column="TRAIN_DOCUMENT"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainVender"
                column="TRAIN_VENDER"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainSpeciality"
                column="TRAIN_SPECIALITY"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainType"
                column="TRAIN_TYPE"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainScore"
                column="TRAIN_SCORE"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainFeedback"
                column="TRAIN_FEEDBACK"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainInternal"
                column="TRAIN_INTERNAL"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

    </class>

</hibernate-mapping>

