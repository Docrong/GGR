	<portlet>
      <portlet-name>JFreeChartDemoPortlet</portlet-name>
      <portlet-class>org.light.portlets.demo.jfreechart.JFreeChartDemoPortlet</portlet-class>

      <supports>
         <mime-type>text/html</mime-type>
         <portlet-mode>VIEW</portlet-mode>
      </supports>
       <resource-bundle>resourceBundle</resource-bundle>
      <portlet-info>
         <title>JFreeChart Demo Portlet</title>
      </portlet-info>
   </portlet>

	<portlet>
