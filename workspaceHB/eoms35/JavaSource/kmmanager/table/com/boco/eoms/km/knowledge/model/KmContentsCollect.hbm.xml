<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.boco.eoms.km.knowledge.model.KmContentsCollect" table="KM_CONTENTS_COLLECT">

		<id name="id" column="id" unsaved-value="null">
			<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
			<generator class="uuid.hex"></generator>
		</id>
		
		<property name="contentId" column="CONTENT_ID" length="32" not-null="false" unique="false">
		</property>
		
		<property name="tableId" column="TABLE_ID" length="32" not-null="false" unique="false">
		</property>
		
		<property name="themeId" column="THEME_ID" length="32" not-null="false" unique="false">
		</property>

		<property name="collectUser" column="COLLECT_USER" length="32" not-null="false" unique="false">
		</property>

		<property name="collectTime" column="COLLECT_TIME" length="7" not-null="false" unique="false">
		</property>

	</class>

</hibernate-mapping>

