<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.boco.eoms.km.log.model.KmOperateLog" table="KM_OPERATE_LOG">
  <id column="id" name="id">
   <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
   <generator class="uuid.hex"/>
  </id>
  <property column="THEME_ID" generated="never" lazy="false" length="32" name="themeId"/>
  <property column="TABLE_ID" generated="never" lazy="false" length="32" name="tableId"/>
  <property column="CONTENT_ID" generated="never" lazy="false"
   length="32" name="contentId"/>
  <property column="OPERATE_DEPT" generated="never" lazy="false"
   length="100" name="operateDept"/>
  <property column="OPERATE_USER" generated="never" lazy="false"
   length="100" name="operateUser"/>
  <property column="SCORE" generated="never" lazy="false" length="20"
   name="score" not-null="true"/>
  <property column="OPERATE_TIME" generated="never" lazy="false"
   length="100" name="operateTime"/>
  <property column="TO_USER" generated="never" lazy="false" length="100" name="toUser"/>
  <property column="OPERATE_ID" generated="never" lazy="false" length="32" name="operateId" />
 </class>
</hibernate-mapping>
