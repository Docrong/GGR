<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.rule.model.KmRule"
            table="KM_RULE"
    >

        <id
                name="id"
                column="id"
                unsaved-value="null"
        >

            <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
            <generator class="uuid.hex">

            </generator>

        </id>

        <property
                name="createUser"
                column="CREATE_USER"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="createDept"
                column="CREATE_DEPT"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="createTime"
                column="CREATE_TIME"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="ruleName"
                column="RULE_NAME"
                length="50"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="ruleScript"
                column="RULE_SCRIPT"
                length="2000"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="ruleParameter"
                column="RULE_PARAMETER"
                length="2000"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="isDeleted"
                column="IS_DELETED"
                length="5"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="remark"
                column="REMARK"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="contentId"
                column="CONTENT_ID"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>


    </class>

</hibernate-mapping>

