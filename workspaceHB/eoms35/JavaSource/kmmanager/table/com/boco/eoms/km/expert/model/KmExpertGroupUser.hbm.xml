<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.boco.eoms.km.expert.model.KmExpertGroupUser"
           table="KM_EXPERT_GROUP_USER">

        <id name="id" column="ID" unsaved-value="null">
            <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
            <generator class="uuid.hex"></generator>
        </id>

        <property name="groupId" column="GROUP_ID" length="32"
                  not-null="false" unique="false">
        </property>

        <property name="userId" column="USER_ID" length="32"
                  not-null="false" unique="false">
        </property>

        <property name="isLeader" column="IS_LEADER" length="32"
                  not-null="false" unique="false">
        </property>
    </class>

</hibernate-mapping>

