<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.score.model.KmScoreWeight"
            table="KM_SCORE_WEIGHT"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="powerName"
            	column="POWER_NAME"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="actionName"
            	column="ACTION_NAME"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="actionWeight"
            	column="ACTION_WEIGHT"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="powerWeight"
            	column="POWER_WEIGHT"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="isDeleted"
            	column="IS_DELETED"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="area"
            	column="AREA"
                length="100"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="remark"
            	column="REMARK"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="nodeId"
            	column="NODEID"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>


    </class>    

</hibernate-mapping>

