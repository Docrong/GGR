<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.boco.eoms.km.expert.model.KmExpertTrain"
		table="KM_EXPERT_TRAIN">

		<id name="id" column="id" unsaved-value="null">

			<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
			<generator class="uuid.hex">

			</generator>

		</id>

		<property name="userId" column="USER_ID" length="32"
			not-null="true" unique="false">

		</property>

		<property name="expertTrainDate" column="EXPERT_TRAIN_DATE"
			length="50" not-null="true" unique="false">

		</property>

		<property name="expertTrainTime" column="EXPERT_TRAIN_TIME"
			length="20" not-null="true" unique="false">

		</property>

		<property name="expertTrainLesson" column="EXPERT_TRAIN_LESSON"
			length="100" not-null="true" unique="false">

		</property>

		<property name="expertCity" column="EXPERT_CITY" length="100"
			not-null="false" unique="false">

		</property>

		<property name="expertUnit" column="EXPERT_UNIT" length="150"
			not-null="true" unique="false">

		</property>

		<property name="expertLicense" column="EXPERT_LICENSE"
			length="150" not-null="false" unique="false">

		</property>

		<property name="expertTrainDes" column="EXPERT_TRAIN_DES"
			length="1000" not-null="false" unique="false">

		</property>


	</class>

</hibernate-mapping>

