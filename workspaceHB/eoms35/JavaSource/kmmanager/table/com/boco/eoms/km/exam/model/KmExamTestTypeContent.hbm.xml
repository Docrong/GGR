<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.exam.model.KmExamTestTypeContent"
            table="KM_EXAM_TEST_TYPE_CONTENT"
    >

        <id
                name="typeContentID"
                column="TYPE_CONTENT_ID"
                unsaved-value="null"
        >

            <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
            <generator class="uuid.hex">

            </generator>

        </id>


        <property
                name="testTypeID"
                column="TEST_TYPE_ID"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="score"
                column="SCORE"
                length="10"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="questionID"
                column="QUESTION_ID"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>


    </class>

</hibernate-mapping>

