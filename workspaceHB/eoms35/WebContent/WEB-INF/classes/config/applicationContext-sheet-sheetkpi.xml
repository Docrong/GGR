<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
        "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

    <bean id="iSheetKpiBaseDao"
          class="com.boco.eoms.sheet.sheetkpi.dao.hibernate.SheetKpiBaseDao"
          autowire="byName">
    </bean>
    <bean id="iSheetKpiBaseManager" parent="txProxyTemplate">
        <property name="target">
            <bean
                    class="com.boco.eoms.sheet.sheetkpi.service.impl.SheetKpiBaseManagerImpl">
                <property name="sheetKpiBaseDao">
                    <ref bean="iSheetKpiBaseDao"/>
                </property>
            </bean>
        </property>
    </bean>


</beans>
