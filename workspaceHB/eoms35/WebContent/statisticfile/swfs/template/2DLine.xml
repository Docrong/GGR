<?xml version="1.0" encoding="GBK"?>
<root>
	<type>
	<system> 
      <refresh enabled="yes"/> 
    </system>  
		<chart type="2DLine"  minimum_value="0" maximum_value="5">
			<values show="no" decimal_places="" thousand_separator="," prefix=""/>
			<animation enabled="yes"/>
			<names show="yes" rotation="0" placement="chart" position="bottom">
               <font type="verdana_embed_tf" color="#808080"/>
            </names>
			<arguments show="no" decimal_places="0"/>
			<line_chart >
			   <block_names enabled="no"/>
			</line_chart>
		</chart>
		<workspace>
			<chart_area width="300" height="150" x="200" y="150" deep="0">
				<border enabled="yes" color="0xB54001"/>
			</chart_area>
			<base_area enabled="no"/>
			<name text=" "/>
			<grid chart="2DLine">
				<values lines_step="5">
					<lines color="0xB9B9B8"/>
					<captions>
						<font size="12" type="Tahoma"/>
					</captions>
					<ticks>
						<major_ticks>
							<lines length="5" color="0xFBF6EF" type="outside"/>
						</major_ticks>
					</ticks>
				</values>
				<arguments lines_step="1">
					<lines color="0xB9B9B8"/>
					<captions enabled="no">
						<font size="12" type="Tahoma"/>
					</captions>
					<ticks>
						<major_ticks type="outside">
							<lines length="5" color="0xFBF6EF" type="outside"/>
						</major_ticks>
					</ticks>
				</arguments>
			</grid>

			<x_axis smart="no"/>
			<y_axis smart="yes"/>
		</workspace>
		
		<legend enabled="yes" x="400" y="50"> 
			<values enabled="no"/>
			<header names="Í¼Àý"/>
			<names width="150"/>
			<scroller enabled="no"/>
			<border enabled="no"/>
			<background enabled="no"/>
		</legend>
	</type>
</root>