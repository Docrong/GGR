<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.sheet.commonfault.model.CommonFaultViSheetInfo"
            table="CommonFault_ViSheetInfo"
    >

        <id
                name="id"
                column="id"

        >

            <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
            <generator class="assigned">

            </generator>

        </id>

        <property name="mainId"/>
        <property name="visId"/>
        <property name="deleted"/>
        <property name="taskId"/>
        <property name="mainSheetAcceptLimit"/>
        <property name="mainSheetCompleteLimit"/>
        <property name="taskSheetAcceptLimit"/>
        <property name="taskSheetCompleteLimit"/>
    </class>

</hibernate-mapping>

