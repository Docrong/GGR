<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.duty.model.TawRmDispatchRecord"
            table="taw_rm_dispatchrecord"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

        <property
            name="fileName"
            	column="file_name"
                length="50"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="fileSource"
            	column="file_source"
                length="50"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="fileProperty"
            	column="file_property"
                length="50"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="time"
            	column="time"
                length="30"
                not-null="false"
                unique="true"
        >

        </property>

        <property
            name="dispatchDeptId"
            	column="dispatch_dept_id"
                length="50"
                not-null="false"
                unique="true"
        >

        </property>

        <property
            name="dispatchDept"
            	column="dispatch_dept"
                length="50"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="dispatcherId"
            	column="dispatcher_id"
                length="30"
                not-null="false"
                unique="true"
        >

        </property>

        <property
            name="dispatcher"
            	column="dispatcher"
                length="30"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="receiver"
            	column="receiver"
                length="30"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="excuteRequest"
            	column="excute_request"
                length="100"
                not-null="false"
                unique="true"
        >

        </property>

        <property
            name="remark"
            	column="remark"
                length="100"
                not-null="false"
                unique="true"
        >

        </property>

        <property
            name="userId"
            	column="user_id"
                length="30"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="roomId"
            	column="room_id"
                length="30"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="workSerial"
            	column="work_serial"
                length="30"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="createTime"
            	column="create_time"
                length="30"
                not-null="true"
                unique="true"
        >

        </property>
    </class>    

</hibernate-mapping>

