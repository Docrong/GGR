<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.workbench.contact.model.TawWorkbenchDeptContact"
            table="taw_workbench_deptcontact"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

        <property
            name="address"
               	column="address"
                length="50"
                not-null="false"
                unique="false"
        >

        </property>

        <property
            name="contactName"
                column="contact_name"
                length="50"
                not-null="false"
                unique="false"
        >

        </property>

        <property
            name="deleted"
               	column="deleted"
                length="8"
                not-null="false"
                unique="false"
        >

        </property>

        <property
            name="deptId"
                column="dept_id"
                length="20"
                not-null="false"
                unique="false"
        >

        </property>

        <property
            name="deptName"
                column="dept_name"
                length="50"
                not-null="false"
                unique="false"
        >

        </property>

        <property
            name="email"
                 column="email"
                length="50"
                not-null="false"
                unique="false"
        >

        </property>

        <property
            name="groupId"
                column="group_id"
                length="50"
                not-null="false"
                unique="false"
        >

        </property>

        <property
            name="position"
                column="position"
                length="50"
                not-null="false"
                unique="false"
        >

        </property>

        <property
            name="tele"
                column="tele"
                length="50"
                not-null="false"
                unique="false"
        >

        </property>

        <property
            name="userId"
                column="user_id"
                length="50"
                not-null="false"
                unique="false"
        >

        </property>        
    

    </class>    

</hibernate-mapping>

