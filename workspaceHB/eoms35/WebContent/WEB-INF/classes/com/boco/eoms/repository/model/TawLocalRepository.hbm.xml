<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.repository.model.TawLocalRepository"
            table="taw_repository"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="province"
            	column="province"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="city"
            	column="city"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="net"
            	column="net"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="netType"
            	column="net_type"
                length="32"
                not-null="false"
                unique="false"
        >
	</property>
	<property
            name="driverTpye"
            	column="driver_Tpye"
                length="32"
                not-null="false"
                unique="false"
        >
	</property>
	<property
            name="company"
            	column="company"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="netModale"
            	column="net_modale"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="hardwareRepository"
            	column="hardware_repository"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="softwareRepository"
            	column="software_repository"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="patch"
            	column="patch"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="networkTime"
            	column="network_time"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="state"
            	column="state"
                length="3"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="deleted"
            	column="deleted"
                length="3"
                not-null="false"
                unique="false"
        >

        </property>

        
    </class>    

</hibernate-mapping>

