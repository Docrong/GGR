<?xml version="1.0" encoding="UTF-8"?>
<WorkFlowRule name="NetFaultProces" description="网管数据保障工单">
  <flow-define id="NetFaultProces" description="网管数据保障工单" tasknamespace="http://NetFaultProces" linkservicename="" dictfilename="" name="NetFaultProces">
    <phase-id id="receive" name="新增工单" type="receive" displaytransferaudit="false" displayconfirm="false" displayreject="false" displaycommonoperate="false">
      <to-phase-id id="DraftTask" role="dealPerformer@" condition="${operateType}==22" operatetype="" stepdisplay="false" workflowdisplay="false" isrejecttask="false" roleid="" commonoperatetype="" name="保存草稿" />
      <to-phase-id id="TaskTask" role="dealPerformer@" condition="${operateType}==0" operatetype="" stepdisplay="false" workflowdisplay="false" isrejecttask="false" roleid="" commonoperatetype="" name="新建派发" />
    </phase-id>
    <phase-id id="DraftTask" name="草稿" type="operate" displaytransferaudit="false" displayconfirm="false" displayreject="false" displaycommonoperate="false">
      <to-phase-id id="TaskTask" role="dealPerformer@" condition="${operateType}==3" operatetype="" stepdisplay="false" workflowdisplay="false" isrejecttask="false" roleid="" commonoperatetype="" name="草稿派单" />
    </phase-id>
    <phase-id id="RejectTask" name="被驳回" type="operate" displaytransferaudit="false" displayconfirm="false" displayreject="false" displaycommonoperate="false">
      <to-phase-id id="TaskTask" role="dealPerformer@" condition="${operateType}==54" operatetype="" stepdisplay="false" workflowdisplay="false" isrejecttask="false" roleid="" commonoperatetype="" name="重新派发" />
    </phase-id>
    <phase-id id="HoldTask" name="待归档" type="operate" displaytransferaudit="false" displayconfirm="false" displayreject="false" displaycommonoperate="false" />
    <phase-id id="TaskTask" name="任务" type="operate" displaytransferaudit="false" displayconfirm="false" displayreject="false" displaycommonoperate="true">
      <to-phase-id id="RejectTask" role="dealPerformer@" condition="${operateType}==17" operatetype="" stepdisplay="false" workflowdisplay="false" isrejecttask="false" roleid="" commonoperatetype="" name="审核不通过" />
      <to-phase-id id="HoldTask" role="dealPerformer@" condition="${operateType}==200" operatetype="" stepdisplay="false" workflowdisplay="false" isrejecttask="false" roleid="" commonoperatetype="" name="审核通过" />
    </phase-id>
  </flow-define>
</WorkFlowRule>

