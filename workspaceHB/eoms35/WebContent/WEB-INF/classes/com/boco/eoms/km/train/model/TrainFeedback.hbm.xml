<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.train.model.TrainFeedback"
            table="KM_TRAIN_FEEDBACK"
    >

        <id
                name="id"
                column="id"
                unsaved-value="null"
        >

            <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
            <generator class="uuid.hex">

            </generator>

        </id>

        <property
                name="feedbackUser"
                column="FEEDBACK_USER"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainFeedbackDept"
                column="TRAIN_FEEDBACK_DEPT"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainFeedbackTel"
                column="TRAIN_FEEDBACK_TEL"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainFeedbackContent"
                column="TRAIN_FEEDBACK_CONTENT"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainFeedbackTime"
                column="TRAIN_FEEDBACK_TIME"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainFeedbackRemark"
                column="TRAIN_FEEDBACK_REMARK"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="trainPlanId"
                column="TRAIN_PLAN_ID"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>


    </class>

</hibernate-mapping>

