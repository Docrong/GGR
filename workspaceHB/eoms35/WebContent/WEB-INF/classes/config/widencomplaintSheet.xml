    <sql>
        <!--待办工单列表查询接口sql-->
        <todoListSql>
            select task.sheetid,
            m.title,
            m.sheetCompleteLimit,
            decode(task.ifWaitForSubTask,'true','待处理回复',decode(task.taskstatus,2,'未接单',8,'已接单未处理','报错')) as taskstatus,
            case task.subTaskFlag when 'true' then 'T2处理(子任务)' when 'false' then 'T2处理' else '其他' end as taskname

            from Widencomplaint_Task task,
            Widencomplaint_Main m
            where m.sendTime>sysdate-40 and ((task.taskOwner = '@userId@') or
            task.taskOwner in (select u.subRoleid
            from taw_system_userrefrole u
            where u.userid = '@userId@'))
            and (task.taskStatus = '2' or task.taskStatus = '8' or task.taskStatus = '1')
            and task.taskDisplayName &lt;&gt; '草稿'
            and task.taskName ='ExcuteHumTask'
            and m.id = task.sheetKey
            and m.deleted &lt;&gt; 1
            and m.status in (0,1)
            and (task.ifWaitForSubTask = 'false' or
            (task.ifWaitForSubTask = 'true' and
            task.id in (select task1.parentTaskId
            from Widencomplaint_Task task1
            where task1.subTaskFlag = 'true'
            and (task1.subTaskDealFalg = 'false' or
            task1.subTaskDealFalg is null)
            and task1.taskStatus = '5')))

        </todoListSql>
        <!--已办工单列表查询接口sql-->
        <finishtodoList>
            select distinct main.*
            from widencomplaint_main main, widencomplaint_task task
            where task.sheetKey = main.id
            and (task.taskOwner = '@userId@' or
            task.taskOwner in
            (select u.subRoleid
            from taw_system_userrefrole u
            where u.userid = '@userId@'))
            and (task.taskStatus = '5' or task.ifWaitForSubTask = 'true')
            and main.deleted &lt;&gt; 1
            @startSime@
            @endSime@
            and main.status = 0
        </finishtodoList>
        <!-- 工单综合查询接口sql -->
        <inquirySheetSql>
            select distinct main.*
            from widencomplaint_main main
            where main.templateFlag &lt;&gt;1
            and(main.title is not null)
            and main.deleted &lt;&gt;'1'
            @timeStart@
            @timeEnd@
            @orderId@
            @orderTitle@
            @ordertype@
            order by main.sendTime desc
        </inquirySheetSql>
        <ccundoListSql>
            select task.sheetid,
            m.title,
            m.sheetCompleteLimit,
            case task.taskstatus when '2' then '未接单' when '8' then '已接单未处理' end as taskstatus,
            case task.taskname when 'cc' then '抄送' when 'ExcuteHumTask' then 'T2处理' else '其他' end as taskname
            from Widencomplaint_Task task,
            Widencomplaint_Main m
            where m.sendTime>sysdate-10 and ((task.taskOwner = '@userId@') or
            task.taskOwner in (select u.subRoleid
            from taw_system_userrefrole u
            where u.userid = '@userId@'))
            and (task.taskStatus = '2' or task.taskStatus = '8' or task.taskStatus = '1')
            and task.taskDisplayName &lt;&gt; '草稿'
            and task.taskName ='cc'
            and m.id = task.sheetKey
            and m.deleted &lt;&gt; 1
            and (task.ifWaitForSubTask = 'false' or
            (task.ifWaitForSubTask = 'true' and
            task.id in (select task1.parentTaskId
            from Widencomplaint_Task task1
            where task1.subTaskFlag = 'true'
            and (task1.subTaskDealFalg = 'false' or
            task1.subTaskDealFalg is null)
            and task1.taskStatus = '5')))
        </ccundoListSql>

