<?xml version="1.0" encoding="UTF-8"?>
<WorkFlowRule name="MaintenanceService" description="维保服务审批工单">
    <flow-define id="MaintenanceService" description="维保服务审批工单" tasknamespace="http://MaintenanceService"
                 linkservicename="" dictfilename="dict-sheet-maintenanceservice" name="MaintenanceService">
        <phase-id id="receive" name="新建工单" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="Confirmation" role="" condition="${operateType}==0" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="系统自动派单"/>
            <to-phase-id id="DraftTask" role="" condition="${operateType}==22" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="保存草稿"/>
            <to-phase-id id="Examine" role="" condition="${operateType}==107" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="人工建单"/>
        </phase-id>
        <phase-id id="DraftTask" name="草稿" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false"/>
        <phase-id id="HoldTask" name="归档" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false"/>
        <phase-id id="Confirmation" name="维保确认" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="Fill" role="" condition="${operateType}==102" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="厂家填报"/>
            <to-phase-id id="HoldTask" role="" condition="${operateType}==106" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="强制归档"/>
        </phase-id>
        <phase-id id="Fill" name="填报" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="Examine" role="" condition="${operateType}==103" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="审核通过"/>
            <to-phase-id id="Confirmation" role="" condition="${operateType}==4" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="驳回"/>
        </phase-id>
        <phase-id id="Examine" name="审核" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="Approval" role="" condition="${operateType}==104" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="审核通过"/>
            <to-phase-id id="Fill" role="" condition="${operateType}==4" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="驳回"/>
            <to-phase-id id="HoldTask" role="" condition="${operateType}==4" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="强制归档"/>
        </phase-id>
        <phase-id id="Approval" name="审批" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="HoldTask" role="" condition="${operateType}==105" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="审批通过"/>
            <to-phase-id id="Examine" role="" condition="${operateType}==4" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="驳回"/>
        </phase-id>
    </flow-define>
</WorkFlowRule>

