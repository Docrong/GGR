<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.extra.supplierkpi.model.TawSupplierkpiDict"
            table="taw_supplierkpi_dict"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

        <property
            name="dictId"
            	column="dict_id"
                length="100"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="dictName"
            	column="dict_name"
                length="50"
                not-null="true"
                unique="false"
        >

        </property>

        <property
            name="dictRemark"
            	column="dict_remark"
                length="240"
                not-null="false"
                unique="false"
        >

        </property>

		<property
            name="parentDictId"
            	column="parent_dict_id"
                length="100"
                not-null="true"
                unique="false"
        >

        </property>
        
        <property
            name="leaf"
            	column="leaf"
                length="2"
                not-null="true"
                unique="false"
        >

        </property>
        
        <property
            name="nodeType"
            	column="node_type"
                length="100"
                not-null="false"
                unique="false"
        >

        </property>
        
    </class>    

</hibernate-mapping>

