<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.workbench.report.model.TawWorkbenchReport"
            table="taw_workbench_report"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

        <property
            name="reportPerson"
               	column="reportPerson"
                length="100"
                not-null="false"
                unique="false"
        >

        </property>
        <property
            name="reportPersonName"
               	column="reportPersonName"
                length="100"
                not-null="false"
                unique="false"
        >

        </property>

        <property
            name="reportTime"
                column="reportTime"
                length="100"
                not-null="false"
                unique="false"
        >

        </property>

        <property
            name="deleted"
               	column="deleted"
                length="8"
                not-null="false"
                unique="false"
        >

        </property>
        
        <property
            name="auditStatus"
               	column="auditStatus"
                length="8"
                not-null="false"
                unique="false"
        >

        </property>

        <property
            name="tomorrowTarget"
                column="tomorrowTarget"
                length="100"
                not-null="false"
                unique="false"
        >

        </property>

        <property
            name="auditer"
                column="auditer"
                length="100"
                not-null="false"
                unique="false"
        >

        </property>
        
        <property
            name="auditName"
                column="auditName"
                length="100"
                not-null="false"
                unique="false"
        >

        </property>

        <property
            name="reportType"
                 column="reportType"
                length="100"
                not-null="false"
                unique="false"
        >

        </property> 
           
        <property
            name="auditTime"
                 column="auditTime"
                length="100"
                not-null="false"
                unique="false"
        >

        </property>   
        
        <property
            name="summary"
                 column="summary"
                length="500"
                not-null="false"
                unique="false"
        >

        </property>     
    

    </class>    

</hibernate-mapping>

