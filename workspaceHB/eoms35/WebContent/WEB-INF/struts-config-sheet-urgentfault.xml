		<forward name="dealReplyReject"
			path="/WEB-INF/pages/wfworksheet/urgentfault/dealReplyReject.jsp" 
			redirect="false" contextRelative="true" />	
		<forward name="accept"
			path="/WEB-INF/pages/wfworksheet/urgentfault/accept.jsp" 
			redirect="false" contextRelative="true" />	
		<forward
	        name="showInterfaceDraftPage"
	        path="/urgentfault.do?method=showDetailPage"
	        redirect="true"		
	      />
	       <forward name="showWorkFlowInstance"
				path="/WEB-INF/pages/wfworksheet/common/workFlowInstance.jsp"
				redirect="false" contextRelative="true" />
			<forward name="getLinkOperatePage"
				path="/WEB-INF/pages/wfworksheet/common/linkOperatePage.jsp" 
				redirect="false" contextRelative="true" />								
		 </action>
	</action-mappings>

	<controller nocache="true" inputForward="true" maxFileSize="2M" />
	<message-resources parameter="config/ApplicationResources-sheet"
		key="sheet" />
	<message-resources
		parameter="config/ApplicationResources-sheet-urgentfault" key="urgentfault" />
	<!-- ========== Validator plugin ===================  -->
	<plug-in className="org.apache.struts.validator.ValidatorPlugIn">
		<set-property property="pathnames"
			value="/WEB-INF/validator-rules.xml,
                             /WEB-INF/validator-rules-custom.xml,
                             /WEB-INF/validation.xml" />
	</plug-in>

</struts-config>