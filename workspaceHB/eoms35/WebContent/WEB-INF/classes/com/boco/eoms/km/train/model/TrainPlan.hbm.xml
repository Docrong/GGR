<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.train.model.TrainPlan"
            table="KM_TRAIN_PLAN"
    >

        <id
                name="id"
                column="id"
                unsaved-value="null"
        >

            <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
            <generator class="uuid.hex">

            </generator>

        </id>

        <property
                name="trainName"
                column="TRAIN_NAME"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainUser"
                column="TRAIN_USER"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainDept"
                column="TRAIN_DEPT"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainTel"
                column="TRAIN_TEL"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainBeginTime"
                column="TRAIN_BEGIN_TIME"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainContent"
                column="TRAIN_CONTENT"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainAddress"
                column="TRAIN_ADDRESS"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainTime"
                column="TRAIN_TIME"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainUnit"
                column="TRAIN_UNIT"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainVender"
                column="TRAIN_VENDER"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainSpeciality"
                column="TRAIN_SPECIALITY"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainType"
                column="TRAIN_TYPE"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="trainRequireNo"
                column="TRAIN_REQUIRE_NO"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="trainEndTime"
                column="TRAIN_END_TIME"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="fileName"
                column="FILE_NAME"
                length="100"
                not-null="false"
                unique="false"
        >

        </property>


    </class>

</hibernate-mapping>

