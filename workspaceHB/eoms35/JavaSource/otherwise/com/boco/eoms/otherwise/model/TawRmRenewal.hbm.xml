<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.otherwise.model.TawRmRenewal"
            table="taw_rm_renewal"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

        <property
            name="testcardId"
            	column="testcard_id"
                length="30"
                not-null="true"
                unique="true"
        >

        </property>

        <property
            name="borrowDate"
            	column="borrow_date"
                length="30"
                not-null="false"
                unique="true"
        >

        </property>

        <property
            name="intendingReturnDate"
            	column="intending_return_date"
                length="30"
                not-null="false"
                unique="true"
        >

        </property>

        <property
            name="renewDate"
            	column="renew_date"
                length="30"
                not-null="false"
                unique="true"
        >

        </property>

        <property
            name="borrowerId"
            	column="borrower_id"
                length="30"
                not-null="true"
                unique="true"
        >

        </property>
        
        <property
            name="userId"
            	column="user_id"
                length="30"
                not-null="true"
                unique="true"
        >

        </property>
        
        <property
            name="remark"
            	column="remark"
                length="100"
                not-null="false"
                unique="true"
        >

        </property>
    </class>    

</hibernate-mapping>

