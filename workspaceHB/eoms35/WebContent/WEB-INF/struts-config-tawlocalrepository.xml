<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">

<struts-config>
	<form-beans> 
		<form-bean
		  name="tawLocalRepositoryForm"
		  type="com.boco.eoms.repository.webapp.form.TawLocalRepositoryForm"
		/>
		<form-bean
		  name="tawLocalRepositoryUpForm"
		  type="com.boco.eoms.repository.webapp.form.TawLocalRepositoryUpForm"
		/>
	</form-beans>
	<!-- ========== Global Exceptions Definitions =================================== -->
    <global-exceptions>
        <exception type="java.lang.Exception" key="errors.general"
			handler="com.boco.eoms.base.webapp.action.ActionExceptionHandler" />
    </global-exceptions>

	<!-- ========== Global Forward Definitions =================================== -->
	<global-forwards>
		<forward
		  name="success"
		  path="/common/success.jsp"
		  redirect="false"
		  contextRelative="true"
		/>
		<forward
		  name="nopriv"
		  path="/common/nopriv.jsp"
		  redirect="false"
		  contextRelative="true"
		/>
		<forward
		  name="fail"
		  path="/common/failure.jsp"
		  redirect="false"
		  contextRelative="true"
		/>
    </global-forwards>

	<!-- ========== Action Mapping Definitions =================================== -->

	<action-mappings>
		<action
		  path="/tawLocalRepositorys"
		  type="com.boco.eoms.repository.webapp.action.TawLocalRepositoryAction"
		  name="tawLocalRepositoryForm"
		  scope="request"
		  input="mainMenu"
		  parameter="method"
		  unknown="false"
		  validate="false"
		>
			<set-property
			  property="cancellable"
			  value="true"
			/>
			<forward
			  name="edit"
			  path="/WEB-INF/pages/tawLocalRepository/tawLocalRepositoryForm.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="list"
			  path="/WEB-INF/pages/tawLocalRepository/tawLocalRepositoryList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="query"
			  path="/WEB-INF/pages/tawLocalRepository/tawLocalRepositoryQuery.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="upgrade"
			  path="/WEB-INF/pages/tawLocalRepository/tawLocalRepositoryUpgradeFrom.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="queryHistory"
			  path="/WEB-INF/pages/tawLocalRepository/tawLocalRepositoryQueryHistory.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="queryResult"
			  path="/WEB-INF/pages/tawLocalRepository/tawLocalRepositoryUpList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="success"
			  path="/WEB-INF/pages/tawLocalRepository/success.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="search"
			  path="/repository/tawLocalRepositorys.do?method=search"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="softquery"
			  path="/WEB-INF/pages/tawLocalRepository/softLocalRepositoryQuery.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="softlist"
			  path="/WEB-INF/pages/tawLocalRepository/softLocalRepositoryList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="softmain"
			  path="/WEB-INF/pages/tawLocalRepository/softLocalRepositorymain.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
		</action>
		
		<action
		  path="/tawLocalRepositoryUps"
		  type="com.boco.eoms.repository.webapp.action.TawLocalRepositoryUpAction"
		  name="tawLocalRepositoryUpForm"
		  scope="request"
		  input="mainMenu"
		  parameter="method"
		  unknown="false"
		  validate="false"
		>
			<set-property
			  property="cancellable"
			  value="true"
			/>
			<forward
			  name="edit"
			  path="/WEB-INF/pages/tawLocalRepository/tawLocalRepositoryUpgradeFrom.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="list"
			  path="/WEB-INF/pages/tawLocalRepository/tawLocalRepositoryList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="query"
			  path="/WEB-INF/pages/tawLocalRepository/tawLocalRepositoryQueryHistory.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="queryResult"
			  path="/WEB-INF/pages/tawLocalRepository/tawLocalRepositoryUpList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="success"
			  path="/WEB-INF/pages/tawLocalRepository/success.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
		</action>
	</action-mappings>
 	<controller nocache="true" inputForward="true" maxFileSize="2M" />
		 <message-resources parameter="config/ApplicationResources"/>
	<!-- ========== Validator plugin ===================  -->
	<!-- 
		<plug-in className="org.apache.struts.validator.ValidatorPlugIn">
		<set-property property="pathnames"
		value="/WEB-INF/validator-rules.xml,
		/WEB-INF/validator-rules-custom.xml,
		/WEB-INF/validation.xml" />
		</plug-in>
	-->
</struts-config>