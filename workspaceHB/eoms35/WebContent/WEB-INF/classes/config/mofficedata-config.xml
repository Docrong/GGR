<?xml version="1.0" encoding="UTF-8"?>
<WorkFlowRule name="MofficeData" description="局数据工单流程">
    <flow-define id="MofficeData" description="局数据工单流程" tasknamespace="http://MofficeData"
                 linkservicename="iMofficeDataLinkManager" dictfilename="dict-sheet-mofficedata.xml" name="MofficeData">
        <phase-id id="receive" name="新建工单" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="DraftTask" role="dealPerformer@58701" condition="${operateType}==22" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="保存草稿"/>
            <to-phase-id id="AuditDataTask" role="dealPerformer@58702" condition="${operateType}==0" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="新建派发"/>
        </phase-id>
        <phase-id id="DraftTask" name="草稿" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="AuditDataTask" role="dealPerformer@58702" condition="${operateType}==3" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="草稿派发"/>
        </phase-id>
        <phase-id id="RejectTask" name="驳回" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="AuditDataTask" role="dealPerformer@58702" condition="${operateType}==54" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="重新派发"/>
        </phase-id>
        <phase-id id="DataCheckTask" name="局数据质检" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="HoldTask" role="dealPerformer@58701" condition="${operateType}==104" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="质检通过"/>
            <to-phase-id id="AutoACheckTask" role="dealPerformer@58705" condition="${operateType}==111" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="质检不通过"/>
        </phase-id>
        <phase-id id="HoldTask" name="归档" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false"/>
        <phase-id id="AuditDataTask" name="局数据工单审批" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="RejectTask" role="dealPerformer@58701" condition="${operateType}==102" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="审批不通过"/>
            <to-phase-id id="AutoCheckTask" role="dealPerformer@58705" condition="${operateType}==109" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="通过"/>
        </phase-id>
        <phase-id id="AutoCheckTask" name="自动校验" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="OfficeMadeTask" role="" condition="${operateType}==114" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="校验通过"/>
            <to-phase-id id="RejectTask" role="" condition="${operateType}==110" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="校验失败"/>
        </phase-id>
        <phase-id id="AutoACheckTask" name="自动校验" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="DataCheckTask" role="" condition="${operateType}==112" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="校验失败"/>
            <to-phase-id id="OfficeMadeTask" role="" condition="${operateType}==113" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="校验通过"/>
        </phase-id>
        <phase-id id="OfficeMadeTask" name="局数据制作" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="DataCheckTask" role="dealPerformer@58701" condition="${operateType}==103" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="制作完成"/>
        </phase-id>
    </flow-define>
</WorkFlowRule>

