<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.ask.model.KmAskReply"
            table="KM_ASK_REPLY"
    >

        <id
                name="id"
                column="id"
                unsaved-value="null"
        >

            <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
            <generator class="uuid.hex">

            </generator>

        </id>

        <property
                name="questionId"
                column="QUESTION_ID"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="answer"
                column="ANSWER"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="conment"
                column="CONMENT"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="isBest"
                column="IS_BEST"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="answerUser"
                column="ANSWER_USER"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="answerDept"
                column="ANSWER_DEPT"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="answerTime"
                column="ANSWER_TIME"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="fileName"
                column="FILE_NAME"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="isAnonymity"
                column="IS_ANONYMITY"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="countVote"
                column="COUNT_VOTE"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

    </class>

</hibernate-mapping>

