<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
        "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
    <bean id="iAlarmsolveDateDao"
          class="com.boco.eoms.sheet.commonfaultAlarm.dao.hibernate.AlarmsolveDateDao"
          autowire="byName">
    </bean>
    <bean id="iAlarmsolveDateConfigDao"
          class="com.boco.eoms.sheet.commonfaultAlarm.dao.hibernate.AlarmsolveDateConfigDaoHibernate"
          autowire="byName">
    </bean>
    <bean id="iAlarmsolveDateManagerImpl" parent="txProxyTemplate">
        <property name="target">
            <bean
                    class="com.boco.eoms.sheet.commonfaultAlarm.service.impl.AlarmsolveDateManagerImpl">
                <property name="alarmsolveDateDao">
                    <ref bean="iAlarmsolveDateDao"/>
                </property>
                <property name="alarmsolveDateConfigDao">
                    <ref bean="iAlarmsolveDateConfigDao"/>
                </property>
                <property name="mainDAO">
                    <ref bean="ICommonFaultMainDao"/>
                </property>
            </bean>
        </property>
    </bean>


</beans>