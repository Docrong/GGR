<?xml version="1.0" encoding="UTF-8"?>
<WorkFlowRule name="CityCustom" description="地市集客业务工单">
    <flow-define id="CityCustom" description="地市集客业务工单" tasknamespace="http://CityCustom" linkservicename=""
                 dictfilename="dict-sheet-citycustom" name="CityCustom">
        <phase-id id="receive" name="新增工单" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="OrderExamine" role="dealPerformer@" condition="${operateType}==0" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="提交申请"/>
        </phase-id>
        <phase-id id="AffirmHumTask" name="回复建单人" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="HoldTask" role="dealPerformer@" condition="${operateType}==111" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="回复通过"/>
            <to-phase-id id="OpenComplete" role="dealPerformer@" condition="${operateType}==112" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="回复不通过"/>
        </phase-id>
        <phase-id id="HoldTask" name="归档" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false"/>
        <phase-id id="OrderExamine" name="订单审核" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="NetOrderExamine" role="dealPerformer@" condition="${operateType}==102" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="审核通过"/>
        </phase-id>
        <phase-id id="NetOrderExamine" name="网管订单审核" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="SchemeDesign" role="dealPerformer@" condition="${operateType}==103" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="审核通过"/>
        </phase-id>
        <phase-id id="SchemeDesign" name="方案设计" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="SubmitExamine" role="dealPerformer@" condition="${operateType}==104" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="设计通过"/>
        </phase-id>
        <phase-id id="Construction" name="施工" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="SubmitCheck" role="dealPerformer@" condition="${operateType}==107" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="施工完成"/>
        </phase-id>
        <phase-id id="SubmitExamine" name="提交审核" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="ExamineScheme" role="dealPerformer@" condition="${operateType}==105" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="审核通过"/>
        </phase-id>
        <phase-id id="SubmitCheck" name="提交验收" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="CheckExamine" role="dealPerformer@" condition="${operateType}==108" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="验收通过"/>
        </phase-id>
        <phase-id id="ExamineScheme" name="审核方案" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="Construction" role="dealPerformer@" condition="${operateType}==106" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="施工"/>
        </phase-id>
        <phase-id id="CheckExamine" name="验收审核" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="OpenComplete" role="dealPerformer@" condition="${operateType}==109" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="审核通过"/>
        </phase-id>
        <phase-id id="OpenComplete" name="开通完成" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="AffirmHumTask" role="dealPerformer@" condition="${operateType}==110" operatetype=""
                         stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype=""
                         name="审核通过"/>
        </phase-id>
    </flow-define>
</WorkFlowRule>

