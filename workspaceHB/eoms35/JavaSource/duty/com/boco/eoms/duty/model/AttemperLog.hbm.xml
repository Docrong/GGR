<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.duty.model.AttemperLog"
            table="taw_rm_attemper_log"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="operTime"
            	column="oper_time"
                length="25"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="userId"
            	column="user_id"
                length="29"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="userName"
            	column="user_name"
                length="20"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="deptId"
            	column="dept_id"
                length="10"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="deptName"
            	column="dept_name"
                length="20"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="type"
            	column="type"
                length="5"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="operName"
            	column="oper_name"
                length="40"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="attemperId"
            	column="attemper_id"
                length="50"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="subAttemperId"
            	column="sub_attemper_id"
                length="50"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="title"
            	column="title"
                length="50"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="intendBeginTime"
            	column="intend_begin_time"
                length="25"
                not-null="false"
                unique="false"
        >

        </property>


    </class>    

</hibernate-mapping>

