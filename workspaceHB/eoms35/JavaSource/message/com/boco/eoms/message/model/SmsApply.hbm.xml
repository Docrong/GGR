        </property>

        <property
                name="deleted"
                length="5"
                not-null="false"
                unique="true"
        >

        </property>

        <property
                name="isCycleSend"
                length="5"
                not-null="false"
                unique="true"
                column="is_cycle_send"
        >

        </property>

        <property
                name="isSendImediat"
                length="10"
                not-null="false"
                unique="true"
                column="is_send_imediat"
        >

        </property>

        <property
                name="isSendNight"
                length="1"
                not-null="false"
                unique="true"
                column="is_send_night"
        >

        </property>

        <property
                name="msgType"
                length="1"
                not-null="false"
                unique="true"
                column="msg_type"
        >

        </property>

        <property
