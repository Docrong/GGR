<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.boco.eoms.km.expert.model.KmExpertAnswer"
           table="KM_EXPERT_ANSWER">

        <id name="id" column="id" unsaved-value="null">

            <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
            <generator class="uuid.hex">

            </generator>

        </id>

        <property name="title" column="TITLE"
                  length="100" not-null="true" unique="false">

        </property>

        <property name="createTime" column="CREATE_TIME"
                  length="20" not-null="true" unique="false">

        </property>

        <property name="createUserId" column="CREATE_USERID"
                  length="32" not-null="true" unique="false">

        </property>

        <property name="answerUserId" column="ANSWER_USERID"
                  length="32" not-null="true" unique="false">

        </property>

        <property name="answerTime" column="ANSWER_TIME"
                  length="20" not-null="false" unique="false">

        </property>

        <property name="type" column="TYPE"
                  length="32" not-null="true" unique="false">

        </property>

        <property name="question" column="QUESTION"
                  length="4000" not-null="false" unique="false">

        </property>

        <property name="questionAtt" column="QUESTION_ATT"
                  length="100" not-null="false" unique="false">

        </property>

        <property name="answer" column="ANSWER"
                  length="4000" not-null="false" unique="false">

        </property>

        <property name="answerUrl" column="ANSWER_URL"
                  length="500" not-null="false" unique="false">

        </property>

        <property name="answerAtt" column="ANSWER_ATT"
                  length="100" not-null="false" unique="false">

        </property>

        <property name="score" column="SCORE"
                  length="32" not-null="false" unique="false">

        </property>

        <property name="scoreRemark" column="SCORE_REMARK"
                  length="4000" not-null="false" unique="false">

        </property>

        <property name="remark" column="REMARK"
                  length="4000" not-null="false" unique="false">

        </property>

        <property name="state" column="STATE"
                  length="32" not-null="false" unique="false">

        </property>

    </class>

</hibernate-mapping>

