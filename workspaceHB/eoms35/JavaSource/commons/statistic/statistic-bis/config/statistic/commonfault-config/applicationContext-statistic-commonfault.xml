                    </value>
                </entry>
            </map>
        </property>

        <!-- 配置quary算法文件路径 key为excel中配置的参数，value对应文件名称  -->
        <property name="queryCongigMap">
            <map>
                <!--oracle-->
                <entry key="commonfault-std_oracle">
                    <value>
                        classpath:config/statistic/commonfault-config/oracle/statistic-config-query-commonfault-std_oracle.xml
                    </value>
                </entry>
                <entry key="commonfault_delay_KPI3_oracle">
                    <value>
