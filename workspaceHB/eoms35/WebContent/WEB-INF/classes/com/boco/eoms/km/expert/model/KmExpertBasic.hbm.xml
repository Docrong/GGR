<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.boco.eoms.km.expert.model.KmExpertBasic"
		table="KM_EXPERT_BASIC">

		<id name="id" column="ID" unsaved-value="null">
			<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
			<generator class="uuid.hex">
			</generator>
		</id>

		<property name="createUser" column="CREATE_USER" length="32"
			not-null="false" unique="false">
		</property>
		
		<property name="createTime" column="CREATE_TIME" length="32"
			not-null="false" unique="false">
		</property>

		<property name="userId" column="USER_ID" length="32"
			not-null="false" unique="false">
		</property>

		<property name="deptId" column="DEPT_ID" length="32"
			not-null="false" unique="false">
		</property>
		
		<property name="phone" column="PHONE" length="32"
			not-null="false" unique="false">
		</property>		
		
		<property name="expertLevel" column="EXPERT_LEVEL" length="32"
			not-null="false" unique="false">
		</property>		
		
		<property name="expertClass" column="EXPERT_CLASS" length="32"
			not-null="false" unique="false">
		</property>	
		
		<property name="specialty" column="SPECIALTY" length="32"
			not-null="false" unique="false">
		</property>		
		
		<property name="area" column="AREA" length="32"
			not-null="false" unique="false">
		</property>		
		
		<property name="intro" column="INTRO" length="2000"
			not-null="false" unique="false">
		</property>

		<property name="email" column="EMAIL" length="100"
			not-null="false" unique="false">
		</property>

		<property name="mobile" column="MOBILE" length="20"
			not-null="false" unique="false">
		</property>

		<property name="position" column="POSITION" length="200"
			not-null="false" unique="false">
		</property>

		<property name="sex" column="SEX" length="1" not-null="false"
			unique="false">
		</property>

		<property name="notes" column="NOTES" length="250"
			not-null="false" unique="false">
		</property>

		<property name="expertNumber" column="EXPERT_NUMBER"
			length="100" not-null="false" unique="false">
		</property>

		<property name="homeSpace" column="HOME_SPACE" length="250"
			not-null="false" unique="false">
		</property>

		<property name="privateJobDate" column="PRIVATE_JOB_DATE"
			length="50" not-null="false" unique="false">
		</property>

		<property name="company" column="COMPANY" length="100"
			not-null="false" unique="false">
		</property>

		<property name="nation" column="NATION" length="40"
			not-null="false" unique="false">
		</property>

		<property name="birthday" column="BIRTHDAY" length="50"
			not-null="false" unique="false">
		</property>

		<property name="polity" column="POLITY" length="40"
			not-null="false" unique="false">
		</property>

		<property name="education" column="EDUCATION" length="40"
			not-null="false" unique="false">
		</property>

		<property name="idCard" column="ID_CARD" length="40"
			not-null="false" unique="false">
		</property>

		<property name="joinJobDate" column="JOIN_JOB_DATE" length="50"
			not-null="false" unique="false">
		</property>

		<property name="isOnline" column="ISONLINE" length="1"
			not-null="false" unique="false">
		</property>

		<property name="imgPath" column="IMG_PATH" length="200"
			not-null="false" unique="false">
		</property>

		<property name="imgPathTemp" column="IMG_PATH_TEMP" length="200"
			not-null="false" unique="false">
		</property>

		<property name="teamLeader" column="TEAM_LEADER" length="1"
			not-null="false" unique="false">
		</property>
	</class>

</hibernate-mapping>