                     path="/WEB-INF/pages/wfworksheet/circuitcontrol/inputreject.jsp"
                     redirect="false" contextRelative="true"/>
            <forward name="detail"
                     path="/WEB-INF/pages/wfworksheet/circuitcontrol/detail.jsp"
                     redirect="false" contextRelative="true"/>
            <forward name="draft"
                     path="/WEB-INF/pages/wfworksheet/circuitcontrol/inputmain.jsp"
                     redirect="false" contextRelative="true"/>
            <forward name="cancel"
                     path="/WEB-INF/pages/wfworksheet/circuitcontrol/inputmain.jsp"
                     redirect="false" contextRelative="true"/>
            <forward name="delete"
                     path="/WEB-INF/pages/wfworksheet/circuitcontrol/inputmain.jsp"
                     redirect="false" contextRelative="true"/>
            <forward name="hold"
                     path="/WEB-INF/pages/wfworksheet/circuitcontrol/inputmain.jsp"
                     redirect="false" contextRelative="true"/>
            <forward name="list"
                     path="/WEB-INF/pages/wfworksheet/circuitcontrol/list.jsp"
                     redirect="false" contextRelative="true"/>
            <forward name="success"
                     path="/WEB-INF/pages/wfworksheet/common/sheet_success.jsp"
                     redirect="false" contextRelative="true"/>
            <forward name="successMessage"
                     path="/WEB-INF/pages/wfworksheet/common/successMessage.jsp"
                     redirect="false" contextRelative="true"/>
            <forward name="portal"
