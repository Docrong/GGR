<?xml version="1.0" encoding="UTF-8"?>
<WorkFlowRule name="AccountPopedomApply" description="账号权限申请流程">
    <flow-define id="AccountPopedomApplyProcess" description="账号权限申请流程">
        <phase-id id="receive" type="receive">
            <to-phase-id id="AuditHumTask" role="dealPerformer@337" condition="${operateType}==0" name="新建派发"/>
            <to-phase-id id="DraftHumTask" role="dealPerformer@337" condition="${operateType}==22" name="保存草稿"/>
        </phase-id>
        <phase-id id="DraftHumTask" name="草稿" type="operate">
            <to-phase-id id="AuditHumTask" role="dealPerformer@337" condition="${operateType}==3" name="草稿派发"/>
        </phase-id>
        <phase-id id="AuditHumTask" name="待审核" type="operate">
            <to-phase-id id="ActualizeHumTask" role="dealPerformer@338" condition="${operateType}==51" name="审核通过"/>
            <to-phase-id id="ByRejectHumTask" role="" condition="${operateType}==52" name="审核不通过"/>
        </phase-id>
        <phase-id id="ByRejectHumTask" name="驳回" type="operate">
            <to-phase-id id="AuditHumTask" role="dealPerformer@337" condition="${operateType}==53" name="重新派发"/>
        </phase-id>
        <phase-id id="ActualizeHumTask" name="实施中" type="operate">
            <to-phase-id id="HoldHumTask" role="dealPerformer@336" condition="${operateType}==18" name="处理完成"/>
        </phase-id>
        <phase-id id="HoldHumTask" name="待归档" type="operate">
        </phase-id>
    </flow-define>
</WorkFlowRule>