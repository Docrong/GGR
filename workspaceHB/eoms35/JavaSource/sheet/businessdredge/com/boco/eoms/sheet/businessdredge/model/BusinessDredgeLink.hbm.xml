        <property name="loginUserName"/>

        <property name="loginUserPassWord"/>
        <!--彩信回复结束  -->
        <!-- 传输专线开始 -->

        <property name="netResCapacity"/>

        <property name="expectFinishdays"/>

        <property name="circuitCode"/>

        <property name="clientPgmCapacity"/>
