<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
        "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">

<struts-config>
    <form-beans>
        <form-bean
                name="partnerUserAndAreaForm"
                type="com.boco.eoms.partner.baseinfo.webapp.form.PartnerUserAndAreaForm"
        />
        <form-bean
                name="areaDeptTreeForm"
                type="com.boco.eoms.partner.baseinfo.webapp.form.AreaDeptTreeForm"
        />
        <form-bean
                name="partnerDeptForm"
                type="com.boco.eoms.partner.baseinfo.webapp.form.PartnerDeptForm"
        />

        <form-bean
                name="partnerUserForm"
                type="com.boco.eoms.partner.baseinfo.webapp.form.PartnerUserForm"
        />
        <form-bean
                name="tawApparatusForm"
                type="com.boco.eoms.partner.baseinfo.webapp.form.TawApparatusForm"
        />
        <form-bean
                name="tawPartnerCarForm"
                type="com.boco.eoms.partner.baseinfo.webapp.form.TawPartnerCarForm"
        />
        <form-bean
                name="tawPartnerOilForm"
                type="com.boco.eoms.partner.baseinfo.webapp.form.TawPartnerOilForm"
        />
    </form-beans>
    <!-- ========== Global Exceptions Definitions =================================== -->
    <global-exceptions>
        <exception type="java.lang.Exception" key="errors.general"
                   handler="com.boco.eoms.base.webapp.action.ActionExceptionHandler"/>
    </global-exceptions>

    <!-- ========== Global Forward Definitions =================================== -->
    <global-forwards>
        <forward
                name="success"
                path="/common/success.jsp"
                redirect="false"
                contextRelative="true"
        />
        <forward
                name="nopriv"
                path="/common/nopriv.jsp"
                redirect="false"
                contextRelative="true"
        />
        <forward
                name="fail"
                path="/common/failure.jsp"
                redirect="false"
                contextRelative="true"
        />
    </global-forwards>

    <!-- ========== Action Mapping Definitions =================================== -->

    <!-- diyubumenshu -->
    <action-mappings>
        <action
                path="/areaDeptTrees"
                type="com.boco.eoms.partner.baseinfo.webapp.action.AreaDeptTreeAction"
                name="areaDeptTreeForm"
                scope="request"
                input="mainMenu"
                parameter="method"
                unknown="false"
                validate="false"
        >
            <set-property
                    property="cancellable"
                    value="true"
            />
            <forward
                    name="tree"
                    path="/WEB-INF/pages/partner/baseinfo/areaDeptTreeTree.jsp"
                    redirect="false"
                    contextRelative="true"
            />
            <forward
                    name="edit"
                    path="/WEB-INF/pages/partner/baseinfo/areaDeptTreeForm.jsp"
                    redirect="false"
                    contextRelative="true"
            />
            <forward
                    name="editFactory"
                    path="/WEB-INF/pages/partner/baseinfo/partnerDeptForm.jsp"
                    redirect="false"
                    contextRelative="true"
            />
            <forward
                    name="selectAreas"
                    path="/WEB-INF/pages/partner/baseinfo/selectAreas.jsp"
                    redirect="false"
                    contextRelative="true"
            />
        </action>

        <!-- renyuandishiyingshebiao -->
        <action
                path="/partnerUserAndAreas"
                type="com.boco.eoms.partner.baseinfo.webapp.action.PartnerUserAndAreaAction"
                name="partnerUserAndAreaForm"
                scope="request"
                input="mainMenu"
                parameter="method"
                unknown="false"
                validate="false"
        >
            <set-property
                    property="cancellable"
                    value="true"
            />
            <forward
                    name="edit"
                    path="/WEB-INF/pages/partner/baseinfo/partnerUserAndArea/partnerUserAndAreaForm.jsp"
                    redirect="false"
                    contextRelative="true"
            />
            <forward
                    name="list"
                    path="/WEB-INF/pages/partner/baseinfo/partnerUserAndArea/partnerUserAndAreaList.jsp"
                    redirect="false"
                    contextRelative="true"
            />
        </action>

        <!-- changshang -->
