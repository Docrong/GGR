<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.kmAuditer.model.KmAuditer"
            table="KM_AUDITER"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="createTime"
            	column="create_time"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="auditType"
            	column="audit_type"
                length="32"
                not-null="true"
                unique="false"
        >
        
        </property>

	<property
            name="nodeId"
            	column="node_id"
                length="32"
                not-null="false"
                unique="false"
        >
        
        </property>
        
	<property
            name="tableId"
            	column="table_id"
                length="32"
                not-null="false"
                unique="false"
        >
        
        </property>
        
	<property
            name="themeId"
            	column="theme_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="roleId"
            	column="role_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="masterId"
            	column="master_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="masterAudit"
            	column="master_audit"
                length="10"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="isSign"
            	column="is_sign"
                length="10"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="remark"
            	column="remark"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>


    </class>    

</hibernate-mapping>

