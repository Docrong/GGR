<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.message.model.SmsApply"
            table="sms_apply"
    >

        <id
                name="id"
                column="id"
                unsaved-value="null"
        >

            <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
            <generator class="uuid.hex">

            </generator>

        </id>

        <property
                name="count"
                length="10"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="endTime"
                length="20"
                not-null="false"
                unique="false"
                column="end_time"
        >

        </property>

        <property
                name="name"
                length="100"
                not-null="false"
                unique="true"
        >

        </property>

        <property
                name="interval"
                length="10"
                not-null="false"
                unique="true"
        >

        </property>

        <property
                name="mobile"
                length="20"
                not-null="false"
                unique="true"
        >

        </property>

        <property
                name="receiverId"
                length="30"
                not-null="false"
                unique="true"
                column="receiver_id"
        >

        </property>

        <property
                name="receiverType"
                length="1"
                not-null="false"
                unique="true"
                column="receiver_type"
        >

        </property>

        <property
                name="regetData"
                length="10"
                not-null="false"
                unique="true"
        >

        </property>

        <property
                name="remark"
                length="200"
                not-null="false"
                unique="true"
        >

        </property>

        <property
                name="serviceId"
                length="32"
                not-null="false"
                unique="true"
                column="service_id"
        >

        </property>

        <property
                name="startTime"
                length="20"
                not-null="false"
                unique="true"
                column="start_time"
        >

        </property>

        <property
                name="userId"
                length="30"
                not-null="false"
                unique="true"
                column="user_id"
        >

        </property>

        <property
                name="cycleHour"
                length="5"
                not-null="false"
                unique="true"
                column="cycle_hour"
        >

        </property>

        <property
                name="cycleMonth"
                length="5"
