          autowire="byName"/>
    <!--TawSystemDeptRefPost-DAO END-->
    <!--TawSystemDeptRefPost-START-->
    <bean id="ItawSystemSubRoleRefPostManager"
          parent="txProxyTemplate">
        <property name="target">
            <bean
