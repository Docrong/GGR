        >

        </property>

        <property
                name="sendStatus"
                length="1"
                column="send_status"
        >

        </property>

        <property
                name="regetAddr"
                length="150"
                column="reget_addr"
        >

        </property>

        <property
                name="regetProtocol"
                length="30"
                column="reget_protocol"
        >

        </property>

        <property
                name="selStatus"
                length="5"
                column="sel_status"
        >

        </property>
    </class>

</hibernate-mapping>

