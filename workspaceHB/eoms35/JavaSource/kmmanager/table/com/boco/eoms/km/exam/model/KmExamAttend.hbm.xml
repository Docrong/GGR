<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.exam.model.KmExamAttend"
            table="KM_EXAM_ATTEND"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="testId"
            	column="TEST_ID"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="attendUser"
            	column="ATTEND_USER"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="attendDept"
            	column="ATTEND_DEPT"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="attendTime"
            	column="ATTEND_TIME"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="attendOverTime"
            	column="ATTEND_OVER_TIME"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="score"
            	column="SCORE"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="isRead"
            	column="IS_READ"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="isPublic"
            	column="IS_PUBLIC"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
    <property
            name="readUser"
            	column="READ_USER"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
       
     <property
            name="readDept"
            	column="READ_DEPT"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
      
    </class>    

</hibernate-mapping>

