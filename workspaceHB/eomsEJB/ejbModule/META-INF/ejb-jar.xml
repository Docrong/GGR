<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar id="ejb-jar_ID" version="2.1" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd">
	<display-name>
	eomsEJB</display-name>
	<enterprise-beans>
		<session id="RoleService">
			<ejb-name>RoleService</ejb-name>
			<home>com.boco.eoms.sheet.base.service.ejb.RoleServiceHome</home>
			<remote>com.boco.eoms.sheet.base.service.ejb.RoleService</remote>
			<ejb-class>com.boco.eoms.sheet.base.service.ejb.RoleServiceBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
		<session id="SaveDataService">
			<ejb-name>SaveDataService</ejb-name>
			<home>com.boco.eoms.sheet.base.service.ejb.SaveDataServiceHome</home>
			<remote>com.boco.eoms.sheet.base.service.ejb.SaveDataService</remote>
			<ejb-class>com.boco.eoms.sheet.base.service.ejb.SaveDataServiceBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
		<session id="MessageService">
			<ejb-name>MessageService</ejb-name>
			<home>com.boco.eoms.sheet.base.service.ejb.MessageServiceHome</home>
			<remote>com.boco.eoms.sheet.base.service.ejb.MessageService</remote>
			<ejb-class>com.boco.eoms.sheet.base.service.ejb.MessageServiceBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
	</enterprise-beans>
	<ejb-client-jar>eomsEJBClient.jar</ejb-client-jar>
</ejb-jar>
