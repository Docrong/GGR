<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.ask.model.KmAskComment"
            table="KM_ASK_COMMENT"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="commentUser"
            	column="COMMENT_USER"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="commentDate"
            	column="COMMENT_DATE"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="commentDept"
            	column="COMMENT_DEPT"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="content"
            	column="CONTENT"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="questionId"
            	column="QUESTION_ID"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        
    </class>    

</hibernate-mapping>

