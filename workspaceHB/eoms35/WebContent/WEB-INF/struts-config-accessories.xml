<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
	<form-beans>
	 <form-bean
        name="tawCommonsAccessoriesConfigForm"
        type="com.boco.eoms.commons.accessories.webapp.form.TawCommonsAccessoriesConfigForm"
      />
     <form-bean
       name="tawCommonsAccessoriesForm"
        type="com.boco.eoms.commons.accessories.webapp.form.TawCommonsAccessoriesForm"
      />
	</form-beans>

	<action-mappings>
	 <action
      path="/tawCommonsAccessoriess"
      type="com.boco.eoms.commons.accessories.webapp.action.TawCommonsAccessoriesAction"
      name="tawCommonsAccessoriesForm"
      scope="request"
      input="mainMenu"
      parameter="method"
      unknown="false"
      validate="false"
    >
      <set-property
        property="cancellable"
        value="true"
      />
      <forward
        name="edit"
        path="/WEB-INF/pages/accessories/tawCommonsAccessories/tawCommonsAccessoriesForm.jsp"
        redirect="false"
        contextRelative="true"
      />
      <forward
        name="list"
        path="/WEB-INF/pages/accessories/tawCommonsAccessories/tawCommonsAccessoriesList.jsp"
        redirect="false"
        contextRelative="true"
      />
      <forward
        name="search"
        path="/tawCommonsAccessoriess.do"
        redirect="true"

      />
       <forward
        name="test"
        path="/WEB-INF/pages/accessories/tawCommonsAccessories/test.jsp"
        redirect="false"
        contextRelative="true"
      />
    </action>
    <action
      path="/editTawCommonsAccessories"
      type="com.boco.eoms.commons.accessories.webapp.action.TawCommonsAccessoriesAction"
      name="tawCommonsAccessoriesForm"
      scope="request"
      input="list"
      parameter="method"
      unknown="false"
      validate="false"
    >
      <set-property
        property="cancellable"
        value="true"
      />
      <forward
        name="edit"
        path="/WEB-INF/pages/accessories/tawCommonsAccessories/tawCommonsAccessoriesForm.jsp"
        redirect="false"
        contextRelative="true"
      />
      <forward
        name="list"
        path="/WEB-INF/pages/accessories/tawCommonsAccessories/tawCommonsAccessoriesList.jsp"
        redirect="false"
        contextRelative="true"
      />
      <forward
        name="search"
        path="/tawCommonsAccessoriess.do"
        redirect="true"
      />
    </action>
    <action
      path="/saveTawCommonsAccessories"
      type="com.boco.eoms.commons.accessories.webapp.action.TawCommonsAccessoriesAction"
      name="tawCommonsAccessoriesForm"
      scope="request"
      input="edit"
      parameter="method"
      unknown="false"
      validate="true"
    >
      <set-property
        property="cancellable"
        value="true"
      />
      <forward
        name="edit"
        path="/WEB-INF/pages/accessories/tawCommonsAccessories/tawCommonsAccessoriesForm.jsp"
        redirect="false"
        contextRelative="true"
      />
      <forward
        name="list"
        path="/WEB-INF/pages/accessories/tawCommonsAccessories/tawCommonsAccessoriesList.jsp"
        redirect="false"
        contextRelative="true"
      />
      <forward
        name="search"
        path="/tawCommonsAccessoriess.do"
        redirect="true"
      />
    </action>
    <action
      path="/reload"
      type="com.boco.eoms.base.webapp.action.ReloadAction"
      roles="admin"
      unknown="false"
      validate="false"
    >
    </action>
    <action
      path="/tawCommonsAccessoriesConfigs"
      type="com.boco.eoms.commons.accessories.webapp.action.TawCommonsAccessoriesConfigAction"
      name="tawCommonsAccessoriesConfigForm"
      scope="request"
      input="mainMenu"
      parameter="method"
      unknown="false"
      validate="false"
    >
      <set-property
        property="cancellable"
        value="true"
      />
      <forward
        name="edit"
        path="/WEB-INF/pages/accessories/tawCommonsAccessoriesConfig/tawCommonsAccessoriesConfigForm.jsp"
        redirect="false"
        contextRelative="true"
      />
      <forward
        name="list"
        path="/WEB-INF/pages/accessories/tawCommonsAccessoriesConfig/tawCommonsAccessoriesConfigList.jsp"
        redirect="false"
        contextRelative="true"
      />
      <forward
        name="search"
        path="/tawCommonsAccessoriesConfigs.do"
        redirect="true"
      />
      <forward
        name="view"
        path="/accessories/pages/view.jsp"
        redirect="false"
        contextRelative="true"
      />
    </action>
    <action
      path="/editTawCommonsAccessoriesConfig"
      type="com.boco.eoms.commons.accessories.webapp.action.TawCommonsAccessoriesConfigAction"
      name="tawCommonsAccessoriesConfigForm"
      scope="request"
      input="list"
      parameter="method"
      unknown="false"
      validate="false"
    >
      <set-property
        property="cancellable"
        value="true"
      />
      <forward
        name="edit"
        path="/WEB-INF/pages/accessories/tawCommonsAccessoriesConfig/tawCommonsAccessoriesConfigForm.jsp"
        redirect="false"
        contextRelative="true"
      />
      <forward
        name="list"
        path="/WEB-INF/pages/accessories/tawCommonsAccessoriesConfig/tawCommonsAccessoriesConfigList.jsp"
        redirect="false"
        contextRelative="true"
      />
      <forward
        name="search"
        path="/tawCommonsAccessoriesConfigs.do"
        redirect="true"
      />
    </action>
    <action
      path="/saveTawCommonsAccessoriesConfig"
      type="com.boco.eoms.commons.accessories.webapp.action.TawCommonsAccessoriesConfigAction"
      name="tawCommonsAccessoriesConfigForm"
      scope="request"
      input="edit"
      parameter="method"
      unknown="false"
      validate="true"
    >
      <set-property
        property="cancellable"
        value="true"
      />
      <forward
        name="edit"
        path="/WEB-INF/pages/accessories/tawCommonsAccessoriesConfig/tawCommonsAccessoriesConfigForm.jsp"
        redirect="false"
        contextRelative="true"
      />
      <forward
        name="list"
        path="/WEB-INF/pages/accessories/tawCommonsAccessoriesConfig/tawCommonsAccessoriesConfigList.jsp"
        redirect="false"
        contextRelative="true"
      />
      <forward
        name="search"
        path="/tawCommonsAccessoriesConfigs.do"
        redirect="true"
      />
    </action>	
 
    <action path="/accessoriesSampleAction"
			type="com.boco.eoms.commons.accessories.sample.AccessoriesSampleAction"
			scope="request" parameter="method" unknown="false" input="mainMenu"
			validate="false">
       <forward name="view"
				path="/WEB-INF/pages/accessories/tawCommonsAccessories/sample/view.jsp" redirect="false"  contextRelative="true"/>
	   <forward name="upload"
				path="/WEB-INF/pages/accessories/tawCommonsAccessories/sample/upload.jsp" redirect="false"  contextRelative="true"/>			
   </action>
  
	</action-mappings>
	
  <controller nocache="true" inputForward="true" maxFileSize="2M" />
  <message-resources parameter="config/ApplicationResources-accessories" />
       <!-- ========== Validator plugin ===================  -->
    <plug-in className="org.apache.struts.validator.ValidatorPlugIn">
        <set-property property="pathnames" 
                      value="/WEB-INF/validator-rules.xml,
                             /WEB-INF/validator-rules-custom.xml,
                             /WEB-INF/validation.xml"/>
    </plug-in>
</struts-config>
