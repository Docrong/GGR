<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.kmAuditerStep.model.KmAuditerStep"
            table="km_auditer_step"
    >

        <id
                name="id"
                column="id"
                unsaved-value="null"
        >

            <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
            <generator class="uuid.hex">

            </generator>

        </id>


        <property
                name="kmId"
                column="km_id"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>


        <property
                name="createTime"
                column="create_time"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="operateTime"
                column="operate_time"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="operateType"
                column="operate_type"
                length="10"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="operateId"
                column="operate_id"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="toOrgType"
                column="to_org_type"
                length="10"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="toOrgId"
                column="to_org_id"
                length="32"
                not-null="true"
                unique="false"
        >
        </property>

        <property
                name="auditResult"
                column="audit_result"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="remark"
                column="remark"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="state"
                column="state"
                length="10"
                not-null="false"
                unique="false"
        >

        </property>

    </class>

</hibernate-mapping>

