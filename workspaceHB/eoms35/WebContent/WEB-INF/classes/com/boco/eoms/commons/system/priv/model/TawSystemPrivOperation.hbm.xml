<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class
		name="com.boco.eoms.commons.system.priv.model.TawSystemPrivOperation"
		table="taw_system_priv_operation">

		<id name="id" column="id" unsaved-value="null">

			<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
			<generator class="uuid.hex">

			</generator>

		</id>

		<property name="code" length="10">

		</property>
		
		<property name="hided" length="10">

		</property>

		<property name="isApp" length="5">

		</property>

		<property name="name" length="50">

		</property> 

		<property name="parentcode" length="10">

		</property>

		<property name="remark" length="200" not-null="false">

		</property>

		<property name="url" length="30" not-null="false">

		</property>
		
		<property name="orderby" length="2" not-null="false">

		</property>
		
		<property name="deleted" length="2" not-null="false"></property> 
		
		<property name="loginType" length="2" not-null="false"></property> 
	</class>

</hibernate-mapping>

