<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
            name="com.boco.eoms.eva.model.EvaEntityRel"
            table="eva_entity_rel"
    >
    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>
		<property
            name="templateId"
            	column="template_id"
                length="255"
                not-null="false"
                unique="false"
        >
        </property>
		<property
            name="templateStatus"
            	column="template_status"
                length="255"
                not-null="false"
                unique="false"
        >
        </property>

    </class>    

</hibernate-mapping>
    