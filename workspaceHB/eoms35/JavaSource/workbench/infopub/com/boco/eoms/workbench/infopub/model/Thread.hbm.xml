<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.boco.eoms.workbench.infopub.model.Thread"
           table="ifp_thread">

        <id name="id" column="id" unsaved-value="null">

            <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
            <generator class="uuid.hex">

            </generator>

        </id>

        <property name="content" length="4096" not-null="false"
                  unique="false" column="content">

        </property>

        <property name="threadCount" length="50" not-null="false"
                  unique="false" column="thread_count">

        </property>

        <property name="createrId" length="50" not-null="false"
                  unique="false" column="creater_id">

        </property>

        <property name="createrName" length="50" not-null="false"
                  unique="false" column="creater_name">

        </property>

        <property name="createTime" length="50" not-null="false"
                  unique="false" column="create_time">

        </property>

        <property name="editTime" length="50" not-null="false"
                  unique="false" column="edit_time">

        </property>

        <property name="forumsId" length="50" not-null="false"
                  unique="false" column="forums_id">
        </property>

        <property name="isDel" length="10" not-null="false"
                  unique="false" column="is_del">

        </property>

        <property name="sortNum" length="10" not-null="false"
                  unique="false" column="sort_num">

        </property>

        <property name="threadTypeId" length="50" not-null="false"
                  unique="false" column="thread_type_id">

        </property>

        <property name="reply" length="10" not-null="false"
                  unique="false" column="reply">

        </property>

        <property name="validity" length="50" not-null="false"
                  unique="false" column="validity">

        </property>

        <property name="isIncludeSubDept" length="10" not-null="false"
                  unique="true" column="is_include_subdept">

        </property>

        <property name="title" length="255" not-null="false"
                  unique="false" column="title">

        </property>

        <property name="status" length="10" not-null="false"
                  unique="false" column="status">

        </property>
        <property name="serialNo" length="40" not-null="false"
                  unique="false" column="serialNo">

        </property>

        <property name="otherSystemType" length="50" not-null="false"
                  unique="true" column="other_system_type">

        </property>

        <property name="interFaceMethod" length="50" not-null="false"
                  unique="true" column="interface_method">

        </property>

        <property name="isSend" length="16" not-null="false"
                  unique="false" column="isSend">

        </property>

        <property name="isAuditSend" length="16" not-null="false"
                  unique="false" column="isAuditSend">

        </property>

        <property name="accessories" length="500" column="ACCESSORIES" not-null="false" type="string"/>
    </class>

</hibernate-mapping>

