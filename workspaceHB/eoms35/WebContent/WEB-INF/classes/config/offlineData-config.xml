<?xml version="1.0" encoding="UTF-8"?>
<WorkFlowRule name="OfflineDataProcess" description="本地任务工单">
  <flow-define id="OfflineDataProcess" description="本地任务工单" tasknamespace="http://OfflineDataProcess" linkservicename="" dictfilename="dict-sheet-OfflineData" name="OfflineData">
    <phase-id id="receive" name="申请" type="receive" displaytransferaudit="false" displayconfirm="false" displayreject="false" displaycommonoperate="false">
      <to-phase-id id="DraftTask" role="" condition="${operateType}==22" operatetype="22" stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="草稿保存" />
      <to-phase-id id="ExcuteHumTask" role="" condition="${operateType}==0" operatetype="0" stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="新建派发" />
    </phase-id>
    <phase-id id="DraftTask" name="草稿" type="operate" displaytransferaudit="false" displayconfirm="false" displayreject="false" displaycommonoperate="false">
      <to-phase-id id="PermitTask" role="" condition="${operateType}==3" operatetype="3" stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="草稿派发" />
    </phase-id>
    <phase-id id="RejectTask" name="被驳回" type="operate" displaytransferaudit="false" displayconfirm="false" displayreject="false" displaycommonoperate="false">
      <to-phase-id id="PermitTask" role="" condition="${operateType}==54" operatetype="54" stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="重新申请" />
    </phase-id>
    <phase-id id="HoldTask" name="归档" type="operate" displaytransferaudit="false" displayconfirm="false" displayreject="false" displaycommonoperate="false" />
    <phase-id id="ExcuteHumTask" name="审批" type="operate" displaytransferaudit="true" displayconfirm="true" displayreject="true" displaycommonoperate="true">
      <to-phase-id id="RejectTask" role="" condition="${operateType}==902" operatetype="902" stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="审核不通过" />
      <to-phase-id id="TaskCreateAuditHumTask" role="" condition="${operateType}==901" operatetype="901" stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="审核通过" />
    </phase-id>
    <phase-id id="TaskCreateAuditHumTask" name="任务执行" type="operate" displaytransferaudit="false" displayconfirm="true" displayreject="true" displaycommonoperate="true">
      <to-phase-id id="HoldTask" role="" condition="${operateType}==91" operatetype="91" stepdisplay="true" workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="资源分配完成" />
    </phase-id>
  </flow-define>
</WorkFlowRule>

