<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.boco.eoms.km.expert.model.KmExpertExp"
		table="KM_EXPERT_EXP">

		<id name="id" column="id" unsaved-value="null">

			<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
			<generator class="uuid.hex">

			</generator>

		</id>

		<property name="expertStart" column="EXPERT_START" length="50"
			not-null="true" unique="false">

		</property>

		<property name="expertEnd" column="EXPERT_END" length="50"
			not-null="true" unique="false">

		</property>

		<property name="expertPosition" column="EXPERT_POSTNAME"
			length="50" not-null="true" unique="false">

		</property>

		<property name="expertAddress" column="EXPERT_ADDRESS"
			length="150" not-null="true" unique="false">

		</property>

		<property name="expertResponsiblitily"
			column="EXPERT_RESPONSIBLITILY" length="500" not-null="false"
			unique="false">

		</property>

		<property name="userId" column="USER_ID" length="32"
			not-null="true" unique="false">

		</property>

		<property name="expertCompany" column="EXPERT_COMPANY"
			length="150" not-null="true" unique="false">

		</property>


	</class>

</hibernate-mapping>

