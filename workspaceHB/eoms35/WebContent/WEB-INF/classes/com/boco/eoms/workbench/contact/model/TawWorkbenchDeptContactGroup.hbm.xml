<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.workbench.contact.model.TawWorkbenchDeptContactGroup"
            table="taw_workbench_deptcontactgroup"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

        <property
            name="deleted"
            	column="deleted"
                not-null="false"
                unique="false"
                length="8"
        >

        </property>

        <property
            name="groupName"
               	column="group_name"
                not-null="false"
                unique="false"
                length="50"
        >

        </property>

        <property
            name="remark"
                column="remark"
                not-null="false"
                unique="false"
                length="255"
        >

        </property>

        <property
            name="userId"
                column="user_id"
                not-null="false"
                unique="false"
                length="25"
        >

        </property>
        
        <property
            name="deptId"
                column="dept_id"
                not-null="false"
                unique="false"
                length="32"
        >

        </property>
        
       <property
            name="groupId"
                column="group_id"
                length="50"
                not-null="false"
                unique="false"
        >

        </property>
        
        <property
            name="allow"
                length="8"
                not-null="false"
                unique="false"
        >

        </property>
    </class>    

</hibernate-mapping>

