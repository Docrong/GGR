<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.duty.model.TawRmArticle"
            table="taw_rm_article"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

        <property
            name="articleName"
                length="128"
                not-null="true"
        >

        </property>

        <property
            name="onlineNum"
                length="32"
                not-null="true"
        >

        </property>

        <property
            name="loanNum"
                length="32"
                not-null="true"
        >

        </property>

        <property
            name="articleType"
                length="128"
                not-null="true"
        >

        </property>

        <property
            name="describe"
                length="512"
                not-null="false"
        >

        </property>
        <property
            name="allNum"
                length="32"
                not-null="true"
        >

        </property>

    </class>    

</hibernate-mapping>

