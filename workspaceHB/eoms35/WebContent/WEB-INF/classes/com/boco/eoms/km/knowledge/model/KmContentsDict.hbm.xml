<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.boco.eoms.km.knowledge.model.KmContentsDict" table="KM_CONTENTS_DICT">

        <id name="id" column="id" unsaved-value="null">
            <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
            <generator class="uuid.hex"></generator>
        </id>

        <property name="contentId" column="CONTENT_ID" length="100" not-null="false" unique="false">
        </property>

        <property name="dictId" column="DICT_ID" length="100" not-null="false" unique="true">
        </property>

        <property name="dictType" column="DICT_TYPE" length="100" not-null="false" unique="false">
        </property>

    </class>

</hibernate-mapping>