		<portlet-info>
			<title>Search Portlet</title>
			<short-title>Search</short-title>
			<keywords>Search</keywords>
        </portlet-info>
	    <portlet-preferences>
            <preference>
				<name>Google</name>
				<value>http://www.google.com/search?q=</value>
				<read-only>true</read-only>
			</preference>
            <preference>
				<name>Yahoo</name>
				<value>http://search.yahoo.com/search?p=</value>
			</preference>
            <preference>
                <name>AskJeeves</name>
                <value>http://web.ask.com/web?q=</value>
            </preference>
            <preference>
				<name>Vivisimo</name>
				<value>http://vivisimo.com/search?query=</value>
			</preference>
			<preference>
				<name>Amazon</name>
				<value>http://www.a9.com/</value>
			</preference>

        </portlet-preferences>

	</portlet>

	<portlet>
        <description>WeatherPortletDescription</description>
        <portlet-name>weatherPortlet</portlet-name>
        <display-name>Weather Portlet</display-name>

        <portlet-class>org.light.portlets.weather.WeatherPortlet</portlet-class>

        <init-param>
            <name>url</name>
            <value>http://xoap.weather.com/weather/local/</value>
        </init-param>

        <init-param>
			<name>partnerId</name>
			<value>1021498252</value>
		</init-param>

		<init-param>
			<name>licence</name>
			<value>4c7efa4d82cd2b90</value>
		</init-param>

        <expiration-cache>-1</expiration-cache>

        <supports>
			<mime-type>text/html</mime-type>
            <portlet-mode>VIEW</portlet-mode>
            <portlet-mode>EDIT</portlet-mode>
            <portlet-mode>HELP</portlet-mode>
            <portlet-mode>CONFIG</portlet-mode>
		</supports>

        <supported-locale>en</supported-locale>
        <supported-locale>fr</supported-locale>
		<resource-bundle>resourceBundle</resource-bundle>
		<portlet-info>
			<title>Weather Portlet</title>
			<short-title>Weather</short-title>
			<keywords>Weather</keywords>
        </portlet-info>

	</portlet>

