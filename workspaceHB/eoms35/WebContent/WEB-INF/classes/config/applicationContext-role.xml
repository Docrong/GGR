                          ref="tawSystemUserRefRoleDao"/>
                <property name="roleDao" ref="tawSystemRoleDao"/>
                <property name="deptDao" ref="tawSystemDeptDao"/>
            </bean>
        </property>
    </bean>
    <!--TawSystemSubRole-END-->
