<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.km.exam.model.KmExamSpecialty"
            table="KM_EXAM_SPECIALTY"
    >

    <id
        name="specialtyID"
            column="SPECIALTY_ID"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="specialtyName"
            	column="SPECIALTY_NAME"
                length="30"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="isDeleted"
            	column="IS_DELETED"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

		<property
            name="nodeId"
            	column="node_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
        <property
            name="parentNodeId"
            	column="parent_node_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
        <property
            name="leaf"
            	column="leaf"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
    </class>    

</hibernate-mapping>

