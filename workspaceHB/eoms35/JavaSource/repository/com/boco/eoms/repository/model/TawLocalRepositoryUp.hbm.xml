<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.repository.model.TawLocalRepositoryUp"
            table="taw_repository_upgrade"
    >

        <id
                name="id"
                column="id"
                unsaved-value="null"
        >

            <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
            <generator class="uuid.hex">

            </generator>

        </id>

        <property
                name="repositoryId"
                column="repository_id"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

        <property
                name="beforeHardwareRepository"
                column="before_hardware_repository"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="beforeSoftwareRepository"
                column="before_software_repository"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="beforePatch"
                column="before_patch"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="hardwareRepository"
                column="hardware_repository"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="softwareRepository"
                column="software_repository"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="patch"
                column="patch"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="content"
                column="content"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="reason"
                column="reason"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="approval"
                column="approval"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

        <property
                name="uptime"
                column="uptime"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        <property
                name="upNum"
                column="up_num"
                not-null="false"
                unique="false"
        >
        </property>
        <property
                name="sheetkey"
                column="sheetkey"
                not-null="false"
                unique="false"
        >
        </property>

    </class>

</hibernate-mapping>

