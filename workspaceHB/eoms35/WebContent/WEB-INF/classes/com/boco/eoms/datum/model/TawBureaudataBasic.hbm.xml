		/>
		<property
			name="prehlrsignalid"
			column="PREHLRSIGNALID"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="hlrsignalid"
			column="HLRSIGNALID"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="newbureauid"
			column="NEWBUREAUID"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="adjustbureauid"
			column="ADJUSTBUREAUID"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="bureadatasheetid"
			column="BUREADATASHEETID"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="sheetid"
			column="SHEETID"
			type="string"
			not-null="false"
			length="100"
		/>


	</class>	
</hibernate-mapping>