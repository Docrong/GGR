<?xml version="1.0" encoding="UTF-8"?>
<WorkFlowRule name="EquipmentUnsubscribe" description="设备退订流程">
    <flow-define id="EquipmentUnsubscribe" description="设备退订流程" tasknamespace="http://EquipmentUnsubscribe"
                 linkservicename="" dictfilename="dict-sheet-equipmentunsubscribe" name="EquipmentUnsubscribe">
        <phase-id id="receive" name="需求申请" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="Audit" role="" condition="${operateType}==0" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="派发"/>
            <to-phase-id id="DraftTask" role="" condition="${operateType}==22" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="保存草稿"/>
        </phase-id>
        <phase-id id="DraftTask" name="草稿" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="Audit" role="" condition="${operateType}==3" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="草稿派发"/>
        </phase-id>
        <phase-id id="HoldTask" name="待归档" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="Dismantle" role="" condition="${operateType}==4" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="驳回"/>
        </phase-id>
        <phase-id id="Audit" name="需求审核" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="Dismantle" role="" condition="${operateType}==101" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="审核通过"/>
            <to-phase-id id="receive" role="" condition="${operateType}==4" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="驳回"/>
        </phase-id>
        <phase-id id="Dismantle" name="施工拆除" type="operate" displaytransferaudit="false" displayconfirm="false"
                  displayreject="false" displaycommonoperate="false">
            <to-phase-id id="HoldTask" role="" condition="${operateType}==102" operatetype="" stepdisplay="true"
                         workflowdisplay="true" isrejecttask="false" roleid="" commonoperatetype="" name="回单"/>
        </phase-id>
    </flow-define>
</WorkFlowRule>

